ACTOR AxeZombie : MagitekMonsterBase
{
	//$Category Monsters
	Game Doom
	Health 30
	Radius 16
	Height 44
    Speed 12
	FastSpeed 15
	PainChance 250
	PainChance "Avoid", 100
	PainChance "Kick", 255
	PainChance "Melee", 255
	PainChance "Stun", 255
	PainChance "Charm", 230
	PainChance "Ice", 255
	Painchance "IceHead", 255
	PainChance "Taunt", 0
	PainChance "KickHead", 255
	damagefactor "KickHead", 1.0
	damagefactor "Fatality", 0.5
	damagefactor "Taunt", 0.0
	damagefactor "Freeze", 0.0
	PainChance "ExplosiveImpact", 255
	MaxStepHeight 24
	MaxDropOffHeight 32
	Monster
	+FLOORCLIP
    +MISSILEMORE
	-TELESTOMP
	+NOTELEFRAG
    Scale 0.98
	MaxStepHeight 24
	MaxDropOffHeight 32
    BloodType "NashGore_Blood"
	SeeSound "grunt/sight"
	PainSound "grunt/pain"
	DeathSound "grunt/death"
	ActiveSound "grunt/active"
    damagefactor "Blood", 0.0 damagefactor "BlueBlood", 0.0 damagefactor "GreenBlood", 0.0
    damagefactor "Avoid", 1.1
    damagefactor "MinorHead", 1.0
	damagefactor "Head", 1.0
	DamageFactor "bloodsplasher", 0.0    DamageFactor "tinybloodsplasher", 0.0    DamageFactor "superbloodsplasher", 0.0    DamageFactor "smallbloodsplasher", 0.0
	DamageFactor "BlueBloodSplasher", 0.0
    DamageFactor "BlueSuperBloodSplasher", 0.0
	damagefactor "Shrapnel", 0.4
	damagefactor "ExplosiveImpact", 3.0
	Obituary "%o met the sharp end of an axe."
	damagefactor "Taunt", 1.0
	//DropItem "BloodyAxe"
	States
	{
    Spawn:
		TNT1 A 0 A_SetAngle(angle + 20)
        TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 5, 40,0)
		SCZA C 8 A_Look
		Loop
	See:
		TNT1 A 0 A_TakeInventory("SKLabguy", 1)
        TNT1 A 0 A_SpawnItemEx("FootStep", 0, 0, 10, 0, 0)
		
		TNT1 A 0 A_SpawnItem ("FastHeadshotTarget2", 5, 40,0)
		SCZA AA 1 A_Chase
		TNT1 A 0
		
		TNT1 A 0 A_SpawnItem ("FastHeadshotTarget2", 5, 40,0)
		SCZA AA 1 A_Chase
		
		TNT1 A 0 A_SpawnItem ("FastHeadshotTarget2", 5, 40,0)
		SCZA BB 1 A_Chase
		TNT1 A 0 A_SpawnItem ("FastHeadshotTarget2", 5, 40,0)
		SCZA BB 1 A_Chase
		
		TNT1 A 0 A_SpawnItem ("FastHeadshotTarget2", 5, 40,0)
		SCZA CC 1 A_Chase
		TNT1 A 0 A_SpawnItem ("FastHeadshotTarget2", 5, 40,0)
		SCZA CC 1 A_Chase
		
		TNT1 A 0 A_SpawnItem ("FastHeadshotTarget2", 5, 40,0)
		SCZA DD 1 A_Chase
		TNT1 A 0 A_SpawnItem ("FastHeadshotTarget2", 5, 40,0)
		SCZA DD 1 A_Chase
		
		
		Loop
		
		Pain.Taunt:
		TNT1 A 0
		TNT1 A 0 A_JumpIfInventory("Enraged", 1, "SpecialAttack")
		POSS B 0 A_Pain
		TNT1 A 0 A_ChangeFlag("FASTER", 1)
		TNT1 A 0 A_ChangeFlag("MISSILEMORE", 1)
		TNT1 A 0 A_ChangeFlag("MISSILEEVENMORE", 1)
		TNT1 A 0 A_PlaySound("grunt/sight")
		SCZA A 1 A_FaceTarget
		SCZA A 1
		TNT1 A 0 A_GiveInventory("Enraged", 1)
		TNT1 A 0 A_PlaySound("grunt/sight", 1)
		Goto SpecialAttack
		
	Melee:
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 0, 40,0)
		SCZA E 8 A_FaceTarget
		TNT1 A 0 A_PlaySound("skeleton/swing")
		TNT1 A 0 A_CustomMissile("ZombieAxeAttack",10,0,0,0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 0, 40,0)
		SCZA F 8 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 0, 40,0)
		SCZA E 10
        Goto See

    

    Pain.KillMe:
        TNT1 A 0
        Goto See
		
    Death.KillMe:
    Death.Taunt:
    Death.Avoid:
	    TNT1 A 0
		TNT1 A 0 A_ChangeFlag("SOLID", 0)
        TNT1 A 0 A_SpawnItem("Labguy")
		TNT1 A 0
        Stop

	Pain:
        TNT1 A 0 A_JumpIfInventory ("IsDown", 1, 7)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 0, 40,0)
		TNT1 A 0 A_CustomMissile ("Nashgore_FlyingBlood", 35, 0, random (0, 360), 2, random (0, 160))
	    SCZA G 3
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 0, 40,0)
		SCZA G 3 A_Pain
		Goto See
        TNT1 AAAA 0
		FSD1 G 4
        TNT1 A 0 A_Pain
        FS11 SSST 10
        TNT1 A 0 A_TakeInventory("IsDown", 1)
        Goto See
    Pain.Melee:
        TNT1 A 0 A_JumpIfInventory ("IsDown", 1, 7)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 0, 40,0)
		TNT1 A 0 A_CustomMissile ("Nashgore_FlyingBlood", 35, 0, random (0, 360), 2, random (0, 160))
	    SCZA G 3
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 0, 40,0)
		SCZA G 3 A_Pain
		Goto See
        TNT1 AAAA 0
		FSD1 G 4
        TNT1 A 0 A_Pain
        FS11 SSST 10
        TNT1 A 0 A_TakeInventory("IsDown", 1)
        Goto See
	Pain.Stun:
		TNT1 A 0 A_ChangeFlag("NOPAIN", 1)
		TNT1 A 0 A_CustomMissile ("StunStarGen", 60, 0, 0, 2 | 4, 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget20", 0, 40,0)
		SCZA G 20 A_Pain
		TNT1 A 0 A_CustomMissile ("StunStarGen", 60, 0, 0, 2 | 4, 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget20", 0, 40,0)
		SCZA G 20
		TNT1 A 0 A_CustomMissile ("StunStarGen", 60, 0, 0, 2 | 4, 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget20", 0, 40,0)
		SCZA G 20 A_Pain
		TNT1 A 0 A_CustomMissile ("StunStarGen", 60, 0, 0, 2 | 4, 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget20", 0, 40,0)
		SCZA G 20
		TNT1 A 0 A_CustomMissile ("StunStarGen", 60, 0, 0, 2 | 4, 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget20", 0, 40,0)
		SCZA G 20 A_Pain
		TNT1 A 0 A_SpawnItem ("HeadshotTarget20", 0, 40,0)
		SCZA G 20
        Goto See	
	Pain.ExplosiveImpact:
	
	Pain.Explosive:
	         TNT1 A 0
	         TNT1 A 0 A_Pain
		 TNT1 A 0 A_JumpIfInventory ("IsDown", 1, "Pain")
		 		    TNT1 A 0 A_TakeInventory("SKLabguy", 1)
         TNT1 A 0 A_GiveInventory("IsDown", 1)
        TNT1 A 0 ThrustThingZ(0,40,0,1)
         FS11 GH 8
		Goto FallingAfterImpact
		
    Pain.Kick:
	Pain.KickHead:
         TNT1 A 0
         FS11 G 1 A_Pain
		 TNT1 A 0 A_JumpIfInventory ("IsDown", 1, "Pain")
		  TNT1 A 0 A_TakeInventory("SKLabguy", 1)
         TNT1 A 0 A_GiveInventory("IsDown", 1)
        TNT1 A 0 ThrustThingZ(0,15,0,1)
		//ID11 E 1 A_Chase ("","")
		FS11 G 1 A_facetarget
        FS11 G 6 ThrustThing(angle*256/360+128, 10, 0, 0)
        FS11 H 8
		Goto FallingAfterImpact
		
		FallingAfterImpact:
		FS11 H 1 A_GiveInventory ("Fallingheight2", 1)
		FS11 H 1 A_CheckFloor ("GetUp")
		NULL A 0 A_JumpIf (momz == 0, "GetUp")
		Loop
		
		GetUp:
		TNT1 A 0
		TNT1 A 0 A_JumpIfInventory ("Fallingheight2", 20, "Death.Stomp")
		TNT1 A 0 A_JumpIfInventory ("Fallingheight2", 14, "FallingSuperDeath")
		TNT1 A 0 A_JumpIfInventory ("Fallingheight2", 7, "FallingDeath")
		FS11 SSSSSSS 5 A_JumpIf(momz < 0, "FallingAfterImpact")
		TNT1 A 0 A_TakeInventory("Fallingheight2", 100)
        TNT1 A 0 A_TakeInventory("IsDown", 1)
		FS11 T 10
         Goto See
		 
		
	FallingDeath:
	TNT1 A 0
	TNT1 A 0 A_Explode(200, 2, 1)
	TNT1 A 0 A_Scream
	TNT1 A 0 A_NoBLocking
	TNT1 AAAA 0 A_CustomMissile ("XDeath1", 32, 0, random (0, 360), 2, random (50, 130))
	FS11 GH 2
	TNT1 A 0 A_SpawnItem ("BigBloodSpot")
	TNT1 A 0 A_Custommissile("BloodyAxe", 32, 0, random (0, 360), 2, random (50, 130))
	FSMS L -1
	Stop
	
	FallingSuperDeath:
	TNT1 A 0
	TNT1 A 0 A_Explode(200, 2, 1)
	TNT1 A 0 A_XScream
	TNT1 A 0 A_NoBLocking
	TNT1 A 0 A_SpawnItem ("BigBloodSpot")
	Goto Death.ExplosiveImpact+1
	

	    Death.Melee:
		TNT1 A 0 A_Jump (95, 2)
		TNT1 A 0 A_SpawnItemEx ("ItemBagHealthBonus", 0, 0, 15)
		TNT1 AAA 0
        TNT1 A 0 A_JumpIfInventory ("IsDown", 1, "Death.Down")
		TNT1 A 0 A_JumpIfInventory ("SKLabguy", 1, "StealthKill")
        Death.Kick:
		TNT1 A 0 ThrustThingZ(0,30,0,1)
		TNT1 A 0 A_Scream
		TNT1 A 0 A_NoBlocking
        TNT1 A 0 A_FaceTarget
        TNT1 A 0 A_Recoil (8)
         FS11 GH 8
		 TNT1 A 0 A_Custommissile("BloodyAxe", 32, 0, random (0, 360), 2, random (50, 130))
		 FSMS L -1
		Stop

		 

   Death.Down:
		POSS I 0 A_Scream
		POSS J 0 A_NoBlocking
		FSMS L 1
		TNT1 A 0 A_Custommissile("BloodyAxe", 32, 0, random (0, 360), 2, random (50, 130))
        FSMS L -1
        Stop


Death.Shotgun:
		TNT1 A 0 A_Jump (95, 2)
		TNT1 A 0 A_SpawnItemEx ("ItemBagHealthBonus", 0, 0, 15)
		TNT1 AAA 0
		TNT1 A 0 A_JumpIfCloser(200, "Death.Shotgunontheface")
		Goto Death
		
	Death.Shotgunontheface:	
	    
		TNT1 A 0
		TNT1 A 0 A_Jump(128, "Death.Arm", "Death.Leg")
	    TNT1 A 0 ThrustThingZ(0,40,0,1)
		TNT1 A 0 A_CustomMissile ("MuchBlood2", 35, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_Scream
		TNT1 A 0 A_NoBlocking
         FS11 GH 8
		 TNT1 A 0 A_Custommissile("BloodyAxe", 32, 0, random (0, 360), 2, random (50, 130))
         FSMS L -1
		 Stop
		 
 	StealthKill:
	TNT1 A 1
	TNT1 A 0 A_GiveToTarget("GoFatality", 1)
	TNT1 A 0 A_GiveToTarget("SKLabguy", 1)
	TNT1 A 0 A_NoBlocking
	TNT1 A 0 A_Die("Stealthy")
	TNT1 A 0 A_Fall
	TNT1 A 1
	Stop
	
	Death.Stealthy:
	TNT1 A 0
	Stop

	Death:
		TNT1 A 0 A_Jump (95, 2)
		TNT1 A 0 A_SpawnItemEx ("ItemBagHealthBonus", 0, 0, 15)
		TNT1 AAA 0
        TNT1 A 0 A_JumpIfInventory ("IsDown", 1, "Death.Down")
        TNT1 A 0
		TNT1 AA 0 A_CustomMissile ("Nashgore_FlyingBlood", 7, 0, random (0, 360), 2, random (0, 160))
        TNT1 A 0 A_Jump(96, "Death.Arm", "Death.Leg")
		TNT1 A 0 A_Jump(128, "DeathMirror")
		FSMS H 5
		FSMS I 5 A_Scream
		FSMS J 5 A_NoBlocking
		FSMS K 5
		TNT1 A 0 A_SpawnItem ("MediumBloodSpot")
		TNT1 A 0 A_Custommissile("BloodyAxe", 32, 0, random (0, 360), 2, random (50, 130))
        FSMS L -1
		Stop
	
	DeathMirror:
	    FSMS M 5
		FSMS N 5 A_Scream
		FSMS O 5 A_NoBlocking
		FSMS P 5
		TNT1 A 0 A_SpawnItem ("MediumBloodSpot")
		TNT1 A 0 A_Custommissile("BloodyAxe", 32, 0, random (0, 360), 2, random (50, 130))
        FSMS P -1
		Stop

     Death.Arm:
		TNT1 A 0 A_CustomMissile ("XDeathArm1", 35, 0, random (0, 360), 2, random (0, 160))
        TNT1 A 0 A_CustomMissile ("MuchBlood2", 35, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_Custommissile("BloodyAxe", 32, 0, random (0, 360), 2, random (50, 130))
		FSD2 A 10 A_Scream
		TNT1 A 0 A_NoBlocking
        TNT1 A 0 A_SpawnItem ("DyingFormerScientistNoArm")
        Stop


	   Death.MinorLeg:
	   Death.LegMassacre:
       Death.Leg:
		TNT1 A 0
        TNT1 A 0 A_CustomMissile ("MuchBlood2", 15, 0, random (0, 360), 2, random (0, 160))
		 TNT1 A 0 A_CustomMissile ("XDeath1", 15, 0, random (0, 360), 2, random (0, 160))
		 TNT1 A 0 A_Custommissile("BloodyAxe", 32, 0, random (0, 360), 2, random (50, 130))
		TNT1 A 0 A_SpawnItem ("Labguy_NoLeg")
