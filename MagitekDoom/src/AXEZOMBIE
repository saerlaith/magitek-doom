	    Death.cutless:
        TNT1 A 0
		TNT1 A 0 A_Jump (95, 2)
		TNT1 A 0 A_SpawnItemEx ("ItemBagHealthBonus", 0, 0, 15)
		TNT1 AAA 0
       TNT1 A 0 A_JumpIfInventory ("IsDown", 1, "Death.Down")
