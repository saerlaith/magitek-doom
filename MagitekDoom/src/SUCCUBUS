actor HeartShot
{
  radius 5
  height 10
  speed 15
  damage (12)
  Scale 1
  renderstyle Translucent
  deathsound "succubus/explode"
  damagetype Succubus
  PROJECTILE
  +RANDOMIZE
  +THRUSPECIES
  +THRUGHOST
  states
  {
  Spawn:
    HART A 1 BRIGHT
    loop
  Death: Crash:
	LMP1 EFG 4 BRIGHT
    stop
  XDeath:
	TNT1 A 0 A_GiveToTarget ("HealingCharge", 1)
	LMP1 EFG 8 BRIGHT
    stop
  }
}

