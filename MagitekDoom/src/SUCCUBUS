	    TNT1 A 0 A_JumpIf(waterlevel > 1, "Death")
        SUCT A 12 A_CustomMissile("BloodTrails",0,0,180,2)
        Goto SpawnOther
    SpawnOther:
        TNT1 A 0 A_SpawnItem("NashGore_BloodSpot",0,0,0,1)
	    TNT1 A 0 A_JumpIf(waterlevel > 1, "Death")
        SUCT D 6 A_CustomMissile("BloodTrails",0,0,180,2)
        Loop
    Death:
        TNT1 A 0 A_SpawnItem("NashGore_BloodSpot",0,0,0,1)
        SUCT C 0 A_SpawnItem("GibHalfSuccubus",0,0,0,1)
        Stop
    }
