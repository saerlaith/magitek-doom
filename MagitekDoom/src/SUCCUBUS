actor PinkBall
{
  spawnid 184
  radius 5
  height 5
  speed 20
  damage (20)
  Scale 2
  renderstyle Translucent
  deathsound "succubus/explode"
  damagetype Succubus
  PROJECTILE
  -NOGRAVITY
  BounceType "Doom"
  Gravity 1
  +RANDOMIZE
  +CANBOUNCEWATER
  +THRUSPECIES
  states
  {
  Spawn:
    LMP1 ABCD 4 BRIGHT
    loop
  Death: Crash:
	LMP1 EFG 4 BRIGHT
    stop
  XDeath:
	TNT1 A 0 A_GiveToTarget ("HealingCharge", 1)
	LMP1 EFG 8 BRIGHT
    stop
  }
}

