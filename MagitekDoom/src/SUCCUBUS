	Death.Melee:
	Death.SuperPunch:
	    SUCA BB 1 A_FaceTarget
		TNT1 A 0 A_CustomMissile ("XDeathSuccubusArm", 5, 0, random (170, 190), 2, random (20, 40))
		Stop
	
	Pain:
	TNT1 A 0
	TNT1 A 0 ThrustThingZ(0,20,0,1)
	SUCA ABCDABCD 3
	Goto Spawn
	
	Death.ExplosiveImpact:
	TNT1 A 0
	TNT1 A 0 A_ChangeFlag("NOBLOCKMAP", 0)
	TNT1 A 0 ThrustThingZ(0,50,0,1)
	SUCA B 5
	SUCA ABCDABCDABCDABCDABCD 5 A_CheckFloor("Landz")
	Goto Landz
	Landz:
	XDT5 I 0 A_SpawnItem("Gib_SuccubusArm")
	Stop	
	}
}

ACTOR HealingCharge : Ammo
