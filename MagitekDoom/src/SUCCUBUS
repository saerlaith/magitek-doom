		Loop
    Death.plasma:	    TNT1 A 0 A_CustomMissile ("XDeathBurnedImpArm", 32, 0, random (0, 360), 2, random (0, 160))        Stop
	Death:
        TNT1 A 0 A_SpawnItem("NashGore_BloodSpot",0,0,0,1)
	    SUCA B 1
        SUCA B 0 A_SpawnItem("Gib_SuccubusArm",0,0,0,1)
        Stop
    }
}

