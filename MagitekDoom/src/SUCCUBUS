	SeeSound "None"
	DeathSound "none"
	Decal "none"
    Speed 40
	States
	{
	Spawn:
		TNT1 A 2 BRIGHT
		Stop
	Death: Crash:
		TNT1 A 0
		stop
	XDeath:
		TNT1 A 0
		TNT1 A 0 A_GiveToTarget ("HealingCharge", 1)
		stop
	}
