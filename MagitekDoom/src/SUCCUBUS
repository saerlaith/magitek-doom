//// --------------------------------------------------------------------------
//
// Succubus
//
// --------------------------------------------------------------------------


actor Succubus : MagitekMonsterBase
{
  Game Doom
  spawnid 186
  obituary "%o couldn't handle the Succubus' love."
  Health 800
  Radius 20
  Height 50
  Speed 8
  Mass 300
  PainChance 150
  PainChance "Shock", 150
  PainChance "ShockForce", 200
  PainChance "Avoid", 100
  PainChance "Stun", 255
  PainChance "Kick", 255
  PainChance "Charm", 100
  PainChance "Ice", 255
  Painchance "IceHead", 255
  PainChance "KickHead", 255
  damagefactor "KickHead", 0.8
  damagefactor "FireHead", 0.8
  damagefactor "IceHead", 0.8
  damagefactor "KickHead", 0.8
  damagefactor "Plasma2", 0.8
  damagefactor "MINORHEAD", 0.8
  damagefactor "HEAD", 0.8
  damagefactor "Freeze", 0.0
  Species Charm
  SeeSound "Succubus/laugh"
  PainSound "Succubus/pain"
  DeathSound "Succubus/Death"
  MaxStepHeight 24
	MaxDropOffHeight 32
  MONSTER
  +FLOORCLIP
  +MISSILEMORE
  states
  {
  Spawn:
	TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 5, 50,0)
    SUCC A 10 A_Look
	TNT1 A 0 A_SetAngle(angle + 20)
	TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 5, 50,0)
	SUCC B 10 A_Look
	TNT1 A 0 A_SetAngle(angle + 20)
    loop
  See:
	TNT1 A 0 A_JumpIfInventory("HealingCharge",1,"HealingState")
	TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 50,0)
    SUCC AA 2 A_Chase
	TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 50,0)
	SUCC AA 2 A_Chase
	TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 50,0)
	SUCC BA 2 A_Chase
	TNT1 A 0 A_Jump (80, "DashRandomly")
	TNT1 A 0 A_Jump (150, "See")
    loop
  DashRandomly:
	TNT1 A 0
	TNT1 A 0 A_Jump (255, "Avoid", "DashBackward", "DashForward")
	Goto See
  HealingState:
	TNT1 A 0 A_Takeinventory("HealingCharge",1)
	TNT1 A 0 A_JumpIfHealthLower(800, 1)
	Goto See
	TNT1 AAA 0
	TNT1 A 0 A_JumpIfInventory("CharmedStrengthPower",1,"HealingStateFriendly")
	TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 45,0)
	TNT1 A 0 A_GiveInventory("Health",25)
	SUCC I 4  A_CustomMissile ("GreenPLSHeal", 30, 0, random (0, 360), 2 | 4, random (-50, 50))
	TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 45,0)
	TNT1 A 0 A_GiveInventory("Health",25)
	SUCC I 4  A_CustomMissile ("GreenPLSHeal2", 30, 0, random (0, 360), 2 | 4, random (-50, 50))
	TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 45,0)
	TNT1 A 0 A_GiveInventory("Health",25)
	SUCC I 4 A_CustomMissile ("GreenPLSHeal", 30, 0, random (0, 360), 2 | 4, random (-50, 50))
	TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 45,0)
	TNT1 A 0 A_GiveInventory("Health",25)
	SUCC I 4 A_CustomMissile ("GreenPLSHeal2", 30, 0, random (0, 360), 2 | 4, random (-50, 50))
	TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 45,0)
	SUCC G 8
	Goto See
  HealingStateFriendly:
	SUCC G 8
	Goto See
  Missile:
	TNT1 A 0 A_Jump(150, "SpecialMissile", "DashForward")
	TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 45,0)
    SUCC G 8 A_FaceTarget
	TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 45,0)
    SUCC G 8
	TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 45,0)
	SUCC I 8 A_FaceTarget
	TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 45,0)
    SUCC I 8
    SUCC J 0 bright A_PlaySound("Succubus/fire")
    SUCC J 0 bright A_CustomMissile ("PinkBall", 27, 15, 8, CMF_OFFSETPITCH, 0)
	SUCC J 0 bright A_CustomMissile ("PinkBall", 27, -15, -8, CMF_OFFSETPITCH, 0)
	TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 45,0)
    SUCC J 4
	SUCC G 4 A_FaceTarget
	TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 45,0)
    SUCC I 4
    SUCC J 0 bright A_PlaySound("Succubus/fire")
    SUCC J 0 bright A_CustomMissile ("PinkBall", 27, 15, 3, CMF_OFFSETPITCH, 10)
	SUCC J 0 bright A_CustomMissile ("PinkBall", 27, -15, -3, CMF_OFFSETPITCH, 10)
	TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 5, 45,0)
    SUCC J 10
    goto See
  SpecialMissile:
	TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 45,0)
	TNT1 A 0 A_CustomMissile ("HeartGen", 40, 0, random (0, 360), 2 | 4, random (-50, 50))
    SUCC G 8 A_FaceTarget
	TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 45,0)
	SUCC I 8 A_FaceTarget
	TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 45,0)
	TNT1 A 0 A_CustomMissile ("HeartGen", 40, 0, random (0, 360), 2 | 4, random (-50, 50))
	SUCC I 8 A_FaceTarget
	TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 45,0)
	SUCC I 8 A_FaceTarget
	TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 45,0)
	SUCC I 8 A_FaceTarget
    SUCC J 0 bright A_PlaySound("Succubus/fire")
    SUCC J 0 bright A_CustomMissile ("HeartShooter", 25, 15, 8, CMF_OFFSETPITCH, 0)
	TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 45,0)
	SUCC J 8
	TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 45,0)
    SUCC G 8
	TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 45,0)
	SUCC G 8
    goto See
  Pain:
	TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 50,0)
    SUCC H 6 A_Pain
	TNT1 A 0 A_Jump (200, "Avoid", "DashBackward")
	Goto See
  Avoid:
	TNT1 A 0
	TNT1 A 0 A_FaceTarget
	TNT1 A 0 A_JumpIfInventory("FrozenStatus",1,"See")
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
    SUCC G 2 A_Chase ("", "", CHF_FASTCHASE)
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
    SUCC G 2 A_Chase ("", "", CHF_FASTCHASE)
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
    SUCC G 2 A_Chase ("", "", CHF_FASTCHASE)
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
    SUCC G 2 A_Chase ("", "", CHF_FASTCHASE)
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
    SUCC G 2 A_Chase ("", "", CHF_FASTCHASE)
	SUCC G 4
    Goto See
  DashForward:
	TNT1 A 0 A_FaceTarget
	TNT1 A 0 A_JumpIfInventory("FrozenStatus",1,"See")
	TNT1 A 0 A_ChangeFlag("NOPAIN", 1)
	TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 2, 50,0)
	SUCC GG 2 A_SpawnItem ("SuccubusFireTrail",0,30)
	TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 2, 50,0)
	SUCC GG 2 A_SpawnItem ("SuccubusFireTrail",0,30)
	TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 2, 50,0)
	SUCC GG 2 A_SpawnItem ("SuccubusFireTrail",0,30)
	TNT1 A 0 A_FaceTarget
	TNT1 A 0 A_Recoil (-20)
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
	SUCC II 1 A_SpawnItem ("SuccubusFireTrail",0,30)
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
	SUCC II 1 A_SpawnItem ("SuccubusFireTrail",0,30)
	TNT1 A 0 A_CustomMissile("SuccubusBump",40,0,0,0)
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
	SUCC II 1 A_SpawnItem ("SuccubusFireTrail",0,30)
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
	SUCC II 1 A_SpawnItem ("SuccubusFireTrail",0,30)
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
	SUCC II 1 A_SpawnItem ("SuccubusFireTrail",0,30)
	TNT1 A 0 A_CustomMissile("SuccubusBump",40,0,0,0)
	TNT1 A 0 A_ChangeFlag("NOPAIN", 0)
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
	SUCC II 1
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
	SUCC JJ 1
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
	SUCC JJ 1
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
	SUCC JJ 1
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
	SUCC JJ 1
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
	SUCC JJ 1
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
	SUCC JJ 1
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
	SUCC JJ 1
	Goto See
  DashBackward:
	TNT1 A 0 A_FaceTarget
	TNT1 A 0 A_JumpIfInventory("FrozenStatus",1,"See")
	TNT1 A 0 A_Recoil (20)
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
	SUCC G 2
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
	SUCC G 2
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
	SUCC G 2
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
	SUCC G 2
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
	SUCC G 2
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
	SUCC G 2
	TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 2, 50,0)
	SUCC G 2
	Goto See
  Pain.Kick:
  Pain.KickHead:
		TNT1 A 0 A_JumpIfInventory("Kicking",1,"Pain.Cancel")
		TNT1 A 0 ThrustThing(angle*256/360+128,4,0,0)
		TNT1 A 0 ThrustThingZ(0,10,0,1)
		TNT1 A 0 A_Giveinventory("Kicking",1)
		TNT1 A 0 A_ChangeFlag("NOPAIN", 1)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 5, 40,0)
        SUC1 B 5 A_Pain
		SUC1 C 5
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 5, 30,0)
		SUC1 G 10
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 5, 30,0)
		SUC1 G 10
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 5, 40,0)
		SUC1 CB 5
		TNT1 A 0 A_ChangeFlag("NOPAIN", 0)
        Goto See
  Pain.Stun:
		TNT1 A 0 A_ChangeFlag("NOPAIN", 1)
		TNT1 A 0 A_CustomMissile ("StunStarGen", 65, 0, 0, 2 | 4, 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget20", 5, 50,0)
		SUCC G 20 A_Pain
		TNT1 A 0 A_CustomMissile ("StunStarGen", 65, 0, 0, 2 | 4, 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget20", 5, 50,0)
		SUCC G 20
		TNT1 A 0 A_CustomMissile ("StunStarGen", 65, 0, 0, 2 | 4, 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget20", 5, 50,0)
		SUCC G 20 A_Pain
		TNT1 A 0 A_CustomMissile ("StunStarGen", 65, 0, 0, 2 | 4, 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget20", 5, 50,0)
		SUCC G 20
		TNT1 A 0 A_CustomMissile ("StunStarGen", 65, 0, 0, 2 | 4, 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget20", 5, 50,0)
		SUCC G 20 A_Pain
		TNT1 A 0 A_SpawnItem ("HeadshotTarget20", 5, 50,0)
		SUCC G 20
        Goto See
  Pain.Explosive:
	TNT1 A 0
	TNT1 A 0 ThrustThingZ(0,40,0,1)
	Goto Pain
  Death:
	SENT A 0 
    SUC1 A 6 A_Scream
    SUC1 ABBBBC 6 A_NoBlocking
	TNT1 A 1 A_SpawnItem ("DyingSuccubus", 5)
    Stop
  Death.MinorHead:
	    TNT1 A 0
		TNT1 A 0 A_Jump (125, 2)
		TNT1 A 0 A_SpawnItemEx ("MagitekChestSpawn", 0, 0, 15)
		TNT1 AAA 0
	    TNT1 A 0 A_Scream
		SUC1 A 8 A_CustomMissile ("MuchBlood2", 40, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAA 0 A_CustomMissile ("Brains1", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAA 0 A_CustomMissile ("Brains3", 50, 0, random (0, 360), 2, random (0, 160))
		SUCH F 8 A_CustomMissile ("MuchBlood2", 40, 0, random (0, 360), 2, random (0, 160))
		SUCH FF 8
		TNT1 AAA 0 A_CustomMissile ("SmallBrainPiece", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("SuperWallRedBlood", 46, 0, random (170, 190), 2, random (0, 40))
		
		SUCH G 15 A_NoBlocking
		TNT1 A 0 A_CustomMissile ("MuchBlood", 30, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_SpawnItem ("MediumBloodSpot")
		SUCH G 15 A_NoBlocking
		TNT1 A 0 A_CustomMissile ("MuchBlood", 30, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_SpawnItem ("MediumBloodSpot")
		SUCH G 15 A_NoBlocking
		TNT1 A 0 A_CustomMissile ("MuchBlood", 30, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_SpawnItem ("MediumBloodSpot")
		SUCH G 15 A_NoBlocking
		TNT1 A 0 A_Pain
		SUCH HI 8 A_NoBlocking
        TNT1 A 1 A_SpawnItem ("DeadSuccubus", 5)
		Stop
	Death.Head: 
	Death.KickHead:
	Death.Plasma2:
		SENT A 0 
		TNT1 A 0 A_Jump (125, 2)
		TNT1 A 0 A_SpawnItemEx ("MagitekChestSpawn", 0, 0, 15)
		TNT1 AAA 0
	    TNT1 A 0 A_Scream
		SUCH A 10 A_CustomMissile ("MuchBlood2", 40, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAAAAAA 0 A_CustomMissile ("Brains1", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("Brains2", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAAAAAA 0 A_CustomMissile ("Brains3", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 AA 0 A_CustomMissile ("Brains4", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 AA 0 A_CustomMissile ("Brains5", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 AA 0 A_CustomMissile ("Brains7", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 AA 0 A_CustomMissile ("Teeth", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 AA 0 A_CustomMissile ("TeethNoBounce", 50, 0, random (0, 360), 2, random (0, 160))
		SUCH A 10 A_CustomMissile ("MuchBlood", 30, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAA 0 A_CustomMissile ("SmallBrainPieceFast", random (45, 55), random (5, -5), random (170, 190), 2, random (-10, 10))
		TNT1 AAAA 0 A_CustomMissile ("SmallBrainPiece", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 AA 0 A_CustomMissile ("SuperWallRedBlood", 46, 0, random (170, 190), 2, random (0, 40))
		SUCH B 10 A_CustomMissile ("MuchBlood", 30, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_SpawnItem ("BigBloodSpot")
		SUCH C 10 A_CustomMissile ("MuchBlood", 30, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_SpawnItem ("BigBloodSpot")
		SUCH C 10 A_CustomMissile ("MuchBlood", 30, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_SpawnItem ("BigBloodSpot")
		SUCH C 10 A_CustomMissile ("MuchBlood", 30, 0, random (0, 360), 2, random (0, 160))
		SUCH C 10 
		TNT1 A 0 A_SpawnItem ("BigBloodSpot")
		TNT1 AAAAAA 0 A_CustomMissile ("MuchBlood", 30, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_SpawnItem ("BigBloodSpot")
		SUCH D 10 A_NoBlocking 
		TNT1 A 1 A_SpawnItem ("DeadSuccubusNoHead", 5)
		Stop
	Death.decaptate:
        TNT1 A 0 A_XScream
		TNT1 A 0 A_Jump (125, 2)
		TNT1 A 0 A_SpawnItemEx ("MagitekChestSpawn", 0, 0, 15)
