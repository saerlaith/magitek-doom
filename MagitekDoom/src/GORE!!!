ACTOR Teeth
{
   Height 2
   Radius 2
   Speed 6
   Scale 0.7
   +DOOMBOUNCE
   - NOGRAVITY
   +WINDTHRUST
   +CLIENTSIDEONLY
   +MOVEWITHSECTOR
   +MISSILE
   +NOBLOCKMAP
   -DROPOFF
   +NOTELEPORT
   +FORCEXYBILLBOARD
   +NOTDMATCH
   +GHOST
   States
   {
   Spawn:
      XTET ABCD 5
      Loop

   Death:
      LCPJ A 0 A_Jump(256,"Rest1","Rest2")
      Goto Rest1
    Rest1:
      XTET B 250
      TNT1 A 0 A_CheckSight("Vanish")
	Rest2:
      XTET D 250
      TNT1 A 0 A_CheckSight("Vanish")
    Loop
  
     Vanish:
     TNT1 A 0
     Stop
   }
}


