	NotchElementalArrow:
		TNT1 A 0 A_JumpIfInventory("MagicType",1,1)
		Goto NotchFlameArrow
		TNT1 A 0 A_JumpIfInventory("MagicType",5,"NotchLightArrow")
		TNT1 A 0 A_JumpIfInventory("MagicType",4,"NotchDarkArrow")
		TNT1 A 0 A_JumpIfInventory("MagicType",3,"NotchEarthArrow")
		TNT1 A 0 A_JumpIfInventory("MagicType",2,"NotchShockArrow")
		TNT1 A 0 A_JumpIfInventory("MagicType",1,"NotchIceArrow")
		goto Ready+10
	NotchFlameArrow:
		TNT1 A 0 A_JumpIfInventory("MagicAmmo",3,1)
		Goto NotchFail
		TNT1 F 0 A_Takeinventory("MagicAmmo",3)
		BOWR ABCDE 1
		TNT1 A 0 A_GiveInventory("ArrowsReady", 3)
		BOWR F 1 A_Refire
		goto Ready+10
