	Reload:
		TNT1 A 0
		TNT1 A 0 A_Takeinventory("Reloading",1)
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",1,"UnnotchArrow")
		Goto NotchElementalArrow
	UnnotchArrow:
		BOWR FEDCBA 2
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",8,"UnnotchLight")
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",7,"UnnotchShadow")
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",6,"UnnotchEarth")
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",5,"UnnotchShock")
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",4,"UnnotchIce")
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",3,"UnnotchFire")
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",2,"UnnotchRaining")
		TNT1 A 0 A_TakeInventory("ArrowsReady", 9)
		TNT1 A 0 A_GiveInventory("MagicAmmo", 1)
		goto Ready+10
	UnnotchLight:
		TNT1 A 0 A_GiveInventory("MagicAmmo", 3)
		TNT1 A 0 A_TakeInventory("ArrowsReady", 9)
		goto Ready+10
	UnnotchShadow:
		TNT1 A 0 A_GiveInventory("MagicAmmo", 3)
		TNT1 A 0 A_TakeInventory("ArrowsReady", 9)
		goto Ready+10
	UnnotchEarth:
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",1,"NoMPUnnotchEarth")
