		TNT1 A 2 LIGHT ("ICEBOUNCEBALL") A_PlaySound("TechBeep",0, 1, 0.1)
		NEDL AAAAAAAAAAAAAAA 1 BRIGHT A_Fire(24)
		TNT1 A 2 LIGHT ("ICEBOUNCEBALL") A_PlaySound("TechBeep",0, 1, 0.1)
		NEDL AAAAAAAAAAAAAAA 1 BRIGHT A_Fire(24)
		TNT1 A 2 LIGHT ("ICEBOUNCEBALL") A_PlaySound("TechBeep",0, 1, 0.1)
		NEDL AAAAAAAAAAAAAAA 1 BRIGHT A_Fire(24)
		
		NEDL A 0 Bright A_Explode(400, 150, false)
		NREB B 0 A_CustomMissile ("InstantFreezeKill", 0, 0, 0, 2 | 4, 0)
		NEDL A 0 A_PlaySound("FAREXPL",0, 1, 0.1)
		TNT1 AAAA 0 A_CustomMissile ("IceBreakGen", 35, 0, random (0, 360), 2 | 4, random (10, 50))
		TNT1 AAAA 0 A_CustomMissile ("IceBreakGen2", 35, 0, random (0, 360), 2 | 4, random (10, 50))
		NREB BBBB 0 A_CustomMissile ("IceCloud", 0, 0, random (0, 360), 2 | 4, random (-70, 70))
		NREB BBBB 0 A_CustomMissile ("IceBoom", 0, 0, random (0, 360), 2 | 4, random (-70, 70))
		Stop
	XDeath:
		NEDL A 0
		NEDL A 0 A_SeekerMissile(90,90,1,255,0)
		SLSH A 0 A_PlaySound("MBow/ArrowHit", 1, 1.0)
		TNT1 A 0 A_ChangeFlag("MISSILE", FALSE)
		TNT1 A 0 A_ChangeFlag("ACTIVATEIMPACT", FALSE)
		TNT1 A 0 A_ChangeFlag("ACTIVATEPCROSS", FALSE)
		TNT1 A 0 A_ChangeFlag("SOLID", FALSE)
		Goto Death+5
	}
