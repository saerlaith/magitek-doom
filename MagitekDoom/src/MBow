ACTOR ShockArrowScatter
{
	Radius 10
	Height 10
	Speed 20
	Damage (100)
	DamageType "HeadshotHitMagicOrb"
	PROJECTILE
	RenderStyle "Add"
	Alpha 1
	+DONTGIB
	Scale 1
	BounceType Hexen
	+BOUNCEONWALLS
	+BOUNCEONFLOORS
	+BOUNCEONCEILINGS
	+CANBOUNCEWATER
	BounceFactor 1
	WallBounceFactor 1
	BounceCount 10
	+DONTGIB
	+NOEXTREMEDEATH
	Translation "0:255=%[0.2,0.2,0]:[1.9,1.9,1.4]"
	States
	{
	Spawn:
	Continue:
		FR7X AB 1 BRIGHT LIGHT ("MIRROR") A_SpawnItem ("ScatterTrail")
		Loop
	Death:
		FR7X ABABABABABABABABABABABABABAB 1 BRIGHT A_FadeOut(0.05)
		Stop
	XDeath:
		TNT1 AAA 0 A_CustomMissile ("ShockSpark2", random (0, 30), random (-30, 30), random (0, 360), 2 | 4, random (-40, 40))
		TNT1 A 0 A_CustomMissile ("ShockBurst2", random (0, 30), random (-30, 30), random (0, 360), 2 | 4, random (-40, 40))
		Stop
	}
}

