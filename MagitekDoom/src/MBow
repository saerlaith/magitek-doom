	ReadyLightArrow:
        TNT1 A 0
		BOWG C 1 A_weaponready
		TNT1 A 0 A_JumpIfInventory("Kicking",1,"DoKick")
		BOWG C 1 A_weaponready
		TNT1 A 0 A_JumpIfInventory("Kicking",1,"DoKick")
		BOWG B 1 A_weaponready
		TNT1 A 0 A_JumpIfInventory("Kicking",1,"DoKick")
		BOWG B 1 A_weaponready
		TNT1 A 0 A_JumpIfInventory("Kicking",1,"DoKick")
		BOWG B 1 A_weaponready
		TNT1 A 0 A_JumpIfInventory("Kicking",1,"DoKick")
		BOWG B 1 A_weaponready
		TNT1 A 0 A_JumpIfInventory("Kicking",1,"DoKick")
		BOWG B 1 A_weaponready
		TNT1 A 0 A_JumpIfInventory("Kicking",1,"DoKick")
		BOWG B 1 A_weaponready
		TNT1 A 0 A_JumpIfInventory("Kicking",1,"DoKick")
		goto ready+35
		
	NotchFail:
		BOWR ABCDB 3
		BOWR A 3 A_Refire
		goto Ready+10
		
	Fire:
	  TNT1 A 0 A_Takeinventory("Reloading",1)
      TNT1 A 0 A_Refire
      Goto Hold
	Hold:
		TNT1 A 0
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",1,"ChargingBow")
		goto NotchBasicArrow
	NotchBasicArrow:
		TNT1 A 0 A_JumpIfInventory("MagicAmmo",1,1)
		Goto NotchFail
		TNT1 A 0 A_TakeInventory("MagicAmmo", 1)
		BOWR ABCDE 1
		TNT1 A 0 A_GiveInventory("ArrowsReady", 1)
		BOWR F 1 A_Refire
		goto Ready+10
		
	ChargingBow:
		TNT1 A 0
		TNT1 A 0 A_JumpIfInventory("Reloading",1,"ZoomCancel")
		TNT1 A 0 A_JumpIfInventory("ChargeShot", 8, "ChargeFull")
		TNT1 A 0 A_JumpIfInventory("ChargeShot", 7, "ChargeMid")
		TNT1 A 0 A_JumpIfInventory("ChargeShot", 6, "ChargeSound")
		TNT1 A 0 A_JumpIfInventory("ChargeShot", 5, "ChargeMid")
		TNT1 A 0 A_JumpIfInventory("ChargeShot", 4, "ChargeMidStart")
		TNT1 A 0 A_JumpIfInventory("ChargeShot", 1, "ChargeSmall")
		TNT1 A 0 A_PlaySound("MBow/Charge", 1, 1)
		BOWC A 4 A_GiveInventory("ChargeShot", 1)
		TNT1 A 0 A_Refire
		goto FireWeak
	ChargeSmall:
		BOWC B 4 A_GiveInventory("ChargeShot", 1)
		TNT1 A 0 A_Refire
		Goto FireWeak
	ChargeMidStart:
		BOWC C 4 A_GiveInventory("ChargeShot", 1)
		TNT1 A 0 A_Refire
		Goto FireMid
	ChargeMid:
		BOWC D 4 A_GiveInventory("ChargeShot", 1)
		TNT1 A 0 A_Refire
		Goto FireMid
	ChargeSound:
		TNT1 A 0 A_PlaySound("Melee/Charge", 0, .9)
		BOWC D 4 A_GiveInventory("ChargeShot", 1)
		TNT1 A 0 A_Refire
		Goto FireMid
	ChargeFull:
		BOWC F 2
		BOWC E 6
		BOWC Q 0 A_Refire
		Goto FireStrong
	ChargeInstant:
		BOWC F 2
		TNT1 A 0 A_JumpIfInventory("Reloading",1,"ZoomCancel")
		BOWC E 6
		BOWC Q 0 A_Refire
		TNT1 A 0 A_JumpIfInventory("ArrowsLeft",1,"FireEarthArrowExtra")
		TNT1 A 0 A_JumpIfInventory("MagicAmmo",2,1)
		Goto Ready
		TNT1 F 0 A_Takeinventory("MagicAmmo",2)
		Goto FireSuper
		
	ZoomCancel:
		BOWG B 20
		TNT1 A 0 A_Takeinventory("Reloading",1)
		Goto Ready+10
	
	AltFire:
		TNT1 A 0
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",1,"InstantShot")
		TNT1 A 0
		goto NotchElementalArrow
	NotchRainingArrow:
		BOWR ABCDEF 1
		TNT1 A 0 A_GiveInventory("ArrowsReady", 2)
		BOWR F 1 A_Refire
		goto Ready+10
		
	InstantShot:
		TNT1 A 0 A_JumpIfInventory("ChargeShot",10,"ChargeInstant")
		TNT1 F 0 A_GiveInventory("ChargeShot", 10)
		Goto ChargeInstant
		
	FireWeak:
		TNT1 A 0 
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",8,"FireLightArrowWeak")
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",7,"FireShadowArrowWeak")
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",6,"FireEarthArrowWeak")
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",5,"FireShockArrowWeak")
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",4,"FireIceArrowWeak")
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",3,"FireFlameArrowWeak")
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",2,"FireRainingWeak")
		NULL A 0 A_JumpIfHealthLower(35, "FireWeakHealing")
		TNT1 A 0 A_TakeInventory("ChargeShot", 999)
		TNT1 A 0 A_TakeInventory("ArrowsReady", 9)
		TNT1 A 0 A_PlaySound("MBow/Fire", 1, 1)
		TNT1 A 0 A_FireCustomMissile("BasicArrowWeak", 0, 0, 0, 5)
		BOWF ABC 4
		Goto Ready+10
	FireMid:
		TNT1 A 0 
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",8,"FireLightArrowMid")
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",7,"FireShadowArrowMid")
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",6,"FireEarthArrowMid")
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",5,"FireShockArrowMid")
