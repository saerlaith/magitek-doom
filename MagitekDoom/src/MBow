		TNT1 A 0 A_JumpIfInventory("ArrowsReady",3,"FireFlameArrowMid")
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",2,"FireRainingMid")
		NULL A 0 A_JumpIfHealthLower(35, "FireMidHealing")
		TNT1 A 0 A_TakeInventory("ChargeShot", 999)
		TNT1 A 0 A_TakeInventory("ArrowsReady", 9)
		TNT1 A 0 A_PlaySound("MBow/Fire", 1, 1)
		TNT1 A 0 BRIGHT A_FireCustomMissile("AlerterMidChance")
		TNT1 A 0 A_FireCustomMissile("BasicArrowMid", 0, 0, 0, 5)
		BOWF ABC 4
		Goto Ready+10
	FireStrong:
		TNT1 A 0 
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",8,"FireLightArrowStrong")
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",7,"FireShadowArrowStrong")
