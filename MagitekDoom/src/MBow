ACTOR EarthArrow
{
  Translation "0:255=%[0.0,0.0,0.0]:[0.4,0.4,0.2]"
  height 5
  radius 5
  damage (25)
  damageType Earth
  speed 30
  Scale 1
  PROJECTILE
  -NOGRAVITY
  Gravity 0.9
  +BLOODSPLATTER
  +NOEXTREMEDEATH
  +THRUGHOST
  +RIPPER
  +BOUNCEONWALLS
  +BOUNCEONCEILINGS
   States
   {
   Spawn:
      NEDL A 0 
      NEDL A 1 
      Loop
    Death:
	  D2:
      NEDL A 0 A_NoGravity
      NEDL A 0 A_JumpIf(waterlevel == 3, 3)
      NEDL A 0 A_PlaySound("MBow/Wall", 1, 1.0)
      Goto Death+8
      NEDL A 0 A_PlaySound("weapons/needleunderwall")
      NEDL AAA 0 A_SpawnItemEx("NeedleBubble",-4,0,0,1,0,1,random(170,190), 128, 0)
      NEDL A 0 A_JumpIf(z > ceilingz-9, "DeathC")
      NEDL A 0 A_JumpIf(z == floorz, "DeathF")
	  TNT1 A 0 A_CustomMissile ("RockBreakPart", 0, 0, random (-90, 90), 2 | 4, random (30, 90))
	  TNT1 A 0 A_CustomMissile ("RockBreakPart2", 0, 0, random (-90, 90), 2 | 4, random (30, 90))
	  TNT1 A 0 A_CustomMissile ("RockBreakPart3", 0, 0, random (-90, 90), 2 | 4, random (30, 90))
      NEDL B 500
      NEDL BBBBBBBBBBBBBBBBBBBB 1 A_FadeOut(0.05)
      Stop
    DeathF:
	  TNT1 A 0 A_Quake(4, 10, 0, 400)
	  TNT1 A 0 A_Explode(100, 150, false, 0, 150)
	  TNT1 AAA 0 A_CustomMissile ("RockBreakPart", 10, 0, random (0, 360), 2 | 4, random (30, 90))
	  TNT1 AAA 0 A_CustomMissile ("RockBreakPart2", 10, 0, random (0, 360), 2 | 4, random (30, 90))
	  TNT1 AAA 0 A_CustomMissile ("RockBreakPart3", 10, 0, random (0, 360), 2 | 4, random (30, 90))
      NEDL C 500
      NEDL CCCCCCCCCCCCCCCCCCCC 1 A_FadeOut(0.05)
      Stop
    DeathC:
	  TNT1 A 0 A_CustomMissile ("RockBreakPart", -10, 0, random (0, 360), 2 | 4, random (30, 90))
	  TNT1 A 0 A_CustomMissile ("RockBreakPart2", -10, 0, random (0, 360), 2 | 4, random (30, 90))
	  TNT1 A 0 A_CustomMissile ("RockBreakPart3", -10, 0, random (0, 360), 2 | 4, random (30, 90))
      NEDL D 500
      NEDL DDDDDDDDDDDDDDDDDDDD 1 A_FadeOut(0.05)
      Stop
    XDeath:
      SLSH A 0 A_PlaySound("MBow/ArrowHit", 1, 1.0)
	  SLSH ABCDEFG 1
      Stop
	Crash:
	  SLSH A 0 A_PlaySound("Melee/Clash", 1, 1.0)
	  SLSH ACEG 1
      Stop
   }
}

