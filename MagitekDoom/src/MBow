		TNT1 A 0 A_JumpIfInventory("ArrowsReady",3,"FireFlameArrowStrong")
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",2,"FireRainingStrong")
		NULL A 0 A_JumpIfHealthLower(35, "FireStrongHealing")
		TNT1 A 0 A_TakeInventory("ChargeShot", 999)
		TNT1 A 0 A_TakeInventory("ArrowsReady", 9)
		TNT1 A 0 A_PlaySound("MBow/Fire", 1, 1)
		TNT1 A 0 BRIGHT A_FireCustomMissile("AlerterMidChance")
		TNT1 A 0 A_FireCustomMissile("BasicArrowStrong", 0, 0, 0, 5)
		BOWF ABC 4
		Goto Ready+10
	FireSuper:
		TNT1 A 0 
		TNT1 A 0 A_PlaySound("Magic/FireAssist")
		TNT1 A 0 A_AlertMonsters (1000)
		TNT1 A 0 A_FireCustomMissile("SuperMissleTrail", 0, 0, 0, 5)
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",8,"FireLightArrowSuper")
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",7,"FireShadowArrowSuper")
		TNT1 A 0 A_JumpIfInventory("ArrowsReady",6,"FireEarthArrowSuper")
