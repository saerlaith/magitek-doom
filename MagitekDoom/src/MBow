ACTOR ShockArrowWeak
{
  Translation "0:255=%[0.5,0.5,0.0]:[1.8,1.8,0.8]"
  height 10
  radius 10
  damage (100)
  damageType MPWeakHit
  speed 40
  Scale 1
  PROJECTILE
  -NOGRAVITY
  Gravity 0.9
  +BLOODSPLATTER
  +NOEXTREMEDEATH
   States
   {
    Spawn:
      TNT1 A 0 
	  FX12 AB 2 BRIGHT A_CustomMissile("ShockBurst2", 0, 0, random (0, 360), 2 | 4, random (-50, 50))
      Loop
    Death:
	  TNT1 A 0 A_ChangeFlag("NOGRAVITY", TRUE)
	  SLSH A 0 A_PlaySound("Melee/Clash", 1, 1.0)
	  NEDL A 0 A_JumpIf(z == floorz, "DeathF")
	  SLSH AAA 0 A_CustomMissile("ShockArrowScatter", 0, 0, random (0, 360), 2 | 4, random (-5, -50))
	  SLSH AAAAAA 0 A_CustomMissile("ShockArrowScatterExtra", 0, 0, random (0, 360), 2 | 4, random (-5, -50))
	  SLSH CEG 1
      Stop
    DeathF:
	  SLSH AAA 0 A_CustomMissile("ShockArrowScatter", 0, 0, random (0, 360), 2 | 4, random (5, 50))
	  SLSH AAAAAA 0 A_CustomMissile("ShockArrowScatterExtra", 0, 0, random (0, 360), 2 | 4, random (5, 50))
	  SLSH CEG 1
      Stop
   }
}

