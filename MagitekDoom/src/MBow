		BOWM B 1 Offset(-70,80) 
		TNT1 A 0 A_jumpifinventory("MagicCooldown",1,3)
		TNT1 A 0 A_PlaySound("MBow/Melee", 1, 1)
		TNT1 A 0 A_FireCustomMissile("ArrowStabHeal", 0, 0, 0, 5)
		TNT1 A 0 A_Jump (256, 3)
		TNT1 A 0
		TNT1 A 0 A_FireCustomMissile("TimeArrowStabHeal", 0, 0, 0, 5)
		BOWM A 1 Offset(-70,60)
		BOWM A 1 Offset(-70,90)
		BOWM A 1 Offset(-70,120)
		BOWM A 1 Offset(-70,150)
		BOWM A 1 Offset(-50,160)
		BOWM B 1 Offset(-60,120)
		BOWM B 1 Offset(-70,80) 
		TNT1 A 0 A_jumpifinventory("MagicCooldown",1,3)
		TNT1 A 0 A_PlaySound("MBow/Melee", 1, 1)
		TNT1 A 0 A_FireCustomMissile("ArrowStabHeal", 0, 0, 0, 5)
		TNT1 A 0 A_Jump (256, 3)
		TNT1 A 0
		TNT1 A 0 A_FireCustomMissile("TimeArrowStabHeal", 0, 0, 0, 5)
		KICK A 0 A_Takeinventory("Kicking",1)
		BOWM A 2 Offset(-70,60)
		BOWM A 12 Offset(-70,70)
		BOWM A 2 Offset(-60,110)
		BOWM A 2 Offset(-50,150)
		BOWM A 2 Offset(-40,190)
		BOWM A 2 Offset(-30,220)
		Goto FinishMeleeBow
		
	FinishMeleeBow:
		TNT1 A 0 A_JumpIfInventory("ArrowsReady", 1, "FinishMeleeNotch")
		BOWG A 1 Offset(3,220)
		BOWG A 1 Offset(3,150)
		BOWG A 1 Offset(3,90)
		BOWG A 1 Offset(3,40)
		Goto Ready+14
		
	FinishMeleeNotch:
		BOWR A 1 Offset(3,220)
		BOWR B 1 Offset(3,150)
		BOWR C 1 Offset(3,110)
		BOWR D 1 Offset(3,60)
		BOWR F 1 Offset(3,30)
		Goto Ready+14
		
    Taunt:
        TNT1 A 5
		FUCK A 2
        FUCK B 2 A_PlaySound("FUCK", 1)
        FUCK C 2 A_AlertMonsters
		FUCK D 15 A_Takeinventory("Taunting",1)
        FUCK CBA 5
        TNT1 A 5
		goto ready+35
	}
}

ACTOR BasicArrowWeak
{
  height 5
  radius 5
  damage (75)
  damageType MPWeakHit
  speed 40
  Scale 1
  PROJECTILE
  -NOGRAVITY
  Gravity 0.9
  +BLOODSPLATTER
  +NOEXTREMEDEATH
  species "Marines"
  +THRUSPECIES
   States
   {
   Spawn:
      NEDL A 0 
      NEDL A 5
      Loop
    Death:
	  D2:
      NEDL A 0 A_NoGravity
      NEDL A 0 A_JumpIf(waterlevel == 3, 3)
      NEDL A 0 A_PlaySound("MBow/Wall", 1, 1.0)
      NEDL A 0 A_CustomMissile("NeedleSmoke",0,0,0)
      Goto Death+8
      NEDL A 0 A_PlaySound("weapons/needleunderwall")
      NEDL AAA 0 A_SpawnItemEx("NeedleBubble",-4,0,0,1,0,1,random(170,190), 128, 0)
      NEDL A 0 A_JumpIf(z > ceilingz-9, "DeathC")
      NEDL A 0 A_JumpIf(z == floorz, "DeathF")
      NEDL B 500
      NEDL BBBBBBBBBBBBBBBBBBBB 1 A_FadeOut(0.05)
      Stop
    DeathF:
      NEDL C 500
      NEDL CCCCCCCCCCCCCCCCCCCC 1 A_FadeOut(0.05)
      Stop
    DeathC:
      NEDL D 500
      NEDL DDDDDDDDDDDDDDDDDDDD 1 A_FadeOut(0.05)
      Stop
    XDeath:
      SLSH A 0 A_PlaySound("MBow/ArrowHit", 1, 1.0)
	  TNT1 A 0 A_GiveToTarget ("MagicAmmo", 1)
	  TNT1 A 0 A_CustomMissile ("BlueManaPoint", 0, 0, random (0, 360), 2 | 4, random (-50, 50))
	  SLSH ABCDEFG 1
      Stop
	Crash:
	  SLSH A 0 A_PlaySound("MBow/Wall", 1, 1.0)
	  SLSH ACEG 1
      Stop
   }
}

