	DoKick:
	    TNT1 A 0 A_WeaponReady
		KICK A 0 A_Takeinventory("Kicking",1)
		TNT1 A 0 A_JumpIfInventory("ArrowsReady", 1, "NotchLower")
		BOWG A 1 Offset(0,90)
		BOWG A 1 Offset(0,150)
		BOWG A 1 Offset(0,220)
		Goto MeleeBasic
		
	NotchLower:
		BOWR E 1 Offset(0,90)
		BOWR D 1 Offset(0,140)
		BOWR C 1 Offset(0,190)
		Goto MeleeBasic
		
	MeleeBasic:
		TNT1 A 0 A_JumpIfHealthLower(35, "MeleeBasicHeal")
		BOWM A 2 Offset(-30,220)
		BOWM A 2 Offset(-40,200)
		BOWM A 1 Offset(-50,160)
		BOWM B 1 Offset(-60,120)
		TNT1 A 0 A_JumpIfInventory("Kicking", 1, "MeleeQuad")
		BOWM B 1 Offset(-70,80) 
		TNT1 A 0 A_jumpifinventory("MagicCooldown",1,3)
		TNT1 A 0 A_PlaySound("MBow/Melee", 1, 1)
		TNT1 A 0 A_FireCustomMissile("ArrowStab", 0, 0, 0, 5)
		TNT1 A 0 A_Jump (256, 3)
		TNT1 A 0
		TNT1 A 0 A_FireCustomMissile("TimeArrowStab", 0, 0, 0, 5)
		KICK A 0 A_Takeinventory("Kicking",1)
		BOWM A 1 Offset(-70,60)
		BOWM A 1 Offset(-60,110)
		BOWM A 1 Offset(-50,150)
		BOWM A 1 Offset(-40,190)
		BOWM A 1 Offset(-30,220)
		Goto FinishMeleeBow
		
