ACTOR RockShardA : ShrapnelShard
{
	Speed 15
	Radius 8
	Scale .4
	Damage (50)
	DamageType Shrapnel
	SeeSound ""
	DeathSound ""
	+THRUACTORS
	-ROCKETTRAIL
	Gravity 1
	BounceCount 15
	BounceFactor 0.7
	WallBounceFactor 0.7
	States
	{
	Spawn:
		ESTN B 10
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
	Continue:
		ESTN B 2 BRIGHT
		Loop
	Death:
		ESTN B 200
		ESTN BBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBB 1 A_FadeOut(0.03)
		Stop
	XDeath:
		TNT1 A 0
		TNT1 A 0 A_PlaySound("")
		ESTN FGH 4
		Stop
  }
}

