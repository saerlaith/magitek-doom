ACTOR EchoRounds : Ammo
{
   Inventory.Amount 0
   Inventory.MaxAmount 4
   Ammo.BackpackAmount 0
   Ammo.BackpackMaxAmount 4
}

ACTOR RefillEcho : Ammo
{
	+INVENTORY.IGNORESKILL
	Inventory.MaxAmount 1
}

//// --------------------------------------------------------------------------
