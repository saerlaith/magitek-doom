ACTOR LightningCanonGen
{
	Radius 5
	Height 5
	Speed 0
	PROJECTILE
	+CEILINGHUGGER
	+DONTGIB
	+RIPPER
	+BLOODLESSIMPACT
	ReactionTime 100
	BounceType Hexen
	BounceFactor 1
	WallBounceFactor 1
	States
	{
	Spawn:
		TNT1 A 0 A_SpawnItemEx ("LightningCanon", random(150, -150), random(150, -150), -20,0,0,0,0,32,0)
		TNT1 A 1 BRIGHT A_CountDown
		Loop
	Death:
		TNT1 A 0
		Stop
	}
}

ACTOR LightningCanon
{
	Radius 10
	Height 3
	Speed 0
	Damage (0)
	DamageType "Shock"
	PROJECTILE
	RenderStyle "Add"
	Alpha 0.7
	+DONTGIB
	+THRUGHOST
	Decal "Scorch"
	Scale 1.2
	-NOGRAVITY
	+DONTGIB
	Gravity 2
	ReactionTime 20
	+NODAMAGETHRUST
	States
	{
	Spawn:
		LITN AB 2 BRIGHT
		TNT1 A 0 A_Countdown
		Loop
	Death:
		TNT1 A 0 A_ChangeFlag("NOGRAVITY", TRUE)
		TNT1 A 0 bright A_Explode (40, 40, false, 40)
		LITN CDE 2 BRIGHT
		TNT1 A 0 bright A_Explode (2, 40, false, 40)
		LITN FGH 2 BRIGHT
		TNT1 AAAA 6 bright A_Explode (2, 40, false, 40)
		Stop
	}
}

