actor LightBlast : CanonShotLow
{
  Radius 9
  MissileType "LightCanonTrail"
  DamageType LightCanon
  Damage (300)
  states
  {
  XDeath:
	TNT1 AAA 0 A_SpawnItemEx ("GreenPLSHeal", random(-20, 20), random(-20, 20), random(-20, 20), 1*random(0,4), 1*random(0,4), 1*random(0,4), Random(0, 360), 129, 0)
	TNT1 AAA 0 A_SpawnItemEx ("GreenPLSHeal2", random(-20, 20), random(-20, 20), random(-20, 20), 1*random(0,4), 1*random(0,4), 1*random(0,4), Random(0, 360), 129, 0)
	TNT1 A 0 A_GiveToTarget ("Health", 10)
  Death:
	TNT1 A 0 Radius_Quake (6, 5, 0, 100, 0)
	SHOK A 0 bright A_Explode (50, 50, true, 50)
	TNT1 A 0 bright A_Explode (450, 50, false, 50)
	TNT1 A 0 A_CustomMissile ("CanonBoomLight", 0, 0, 0, 2 | 4, 0)
	TNT1 AAAAA 0 A_SpawnItemEx ("HolyExplosionPart2", random(-20, 20), random(-20, 20), random(-20, 20), 1*random(0,4), 1*random(0,4), 1*random(0,4), Random(0, 360), 129, 0)
	stop
  }
}