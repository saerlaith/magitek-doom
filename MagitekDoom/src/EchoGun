{
	States
	{
	Spawn:
		ESTN C 10
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
	Continue:
		ESTN C 2 BRIGHT
		Loop
	Death:
		ESTN C 200
		ESTN CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC 1 A_FadeOut(0.03)
		Stop
  }
}

