		TNT1 A 0 A_JumpIfInventory("MagicType",3,"AltFireEarth")
		TNT1 A 0 A_JumpIfInventory("MagicType",2,"AltFireShock")
		TNT1 A 0 A_JumpIfInventory("MagicType",1,"AltFireIce")
		
