		TNT1 A 0 A_Refire
        Goto Ready+7
		
	Refill:
		TNT1 A 0 A_TakeInventory("RefillEcho",1)
		TNT1 A 0 A_JumpIfInventory("EchoRounds",1,2)
		Goto ReadyEmpty
        TNT1 AAAA 0
		
		TNT1 A 0 A_PlaySound("EchoGun/Refill")
		TNT1 A 0 A_JumpIfInventory("MagicType",1,1)
		Goto RefillFire
		TNT1 A 0 A_JumpIfInventory("MagicType",5,"RefillLight")
		TNT1 A 0 A_JumpIfInventory("MagicType",4,"RefillDark")
		TNT1 A 0 A_JumpIfInventory("MagicType",3,"RefillEarth")
		TNT1 A 0 A_JumpIfInventory("MagicType",2,"RefillShock")
		TNT1 A 0 A_JumpIfInventory("MagicType",1,"RefillIce")
	RefillFire:
		1SCC ABCDE 4 BRIGHT
        TNT1 A 0 A_Refire
		Goto Ready+7
	RefillIce:
		2SCC ABCDE 4 BRIGHT
        TNT1 A 0 A_Refire
		Goto Ready+7
	RefillShock:
		3SCC ABCDE 4 BRIGHT
        TNT1 A 0 A_Refire
		Goto Ready+7
	RefillEarth:
		4SCC ABCDE 4 BRIGHT
        TNT1 A 0 A_Refire
		Goto Ready+7
	RefillDark:
		5SCC ABCDE 4 BRIGHT
        TNT1 A 0 A_Refire
		Goto Ready+7
	RefillLight:
		6SCC ABCDE 4 BRIGHT
        TNT1 A 0 A_Refire
		Goto Ready+7

    Reload:
		TNT1 A 0 A_Takeinventory("Reloading",1)
        TNT1 A 0 A_JumpIfInventory("EchoRounds",4,"Ready")
        TNT1 A 0 A_JumpIfInventory("N_RocketAmmo",1,3)
        Goto Ready
		TNT1 AAA 0
		
		TNT1 A 0 A_JumpIfInventory("EchoRounds",1,3)
		Goto ReloadStart
		TNT1 AAA 0
		
		TNT1 A 0 A_JumpIfInventory("MagicType",1,1)
		Goto ReloadFire
		TNT1 A 0 A_JumpIfInventory("MagicType",5,"ReloadLight")
		TNT1 A 0 A_JumpIfInventory("MagicType",4,"ReloadDark")
		TNT1 A 0 A_JumpIfInventory("MagicType",3,"ReloadEarth")
		TNT1 A 0 A_JumpIfInventory("MagicType",2,"ReloadShock")
		TNT1 A 0 A_JumpIfInventory("MagicType",1,"ReloadIce")
		
	ReloadFire:
		1SCC EDCBA 2
		Goto ReloadStart
	ReloadIce:
		2SCC EDCBA 2
		Goto ReloadStart
	ReloadShock:
		3SCC EDCBA 2
		Goto ReloadStart
	ReloadEarth:
		4SCC EDCBA 2
		Goto ReloadStart
	ReloadDark:
		5SCC EDCBA 2
		Goto ReloadStart
	ReloadLight:
		6SCC EDCBA 2
		Goto ReloadStart
	ReloadStart:
		0SCR A 2
		TNT1 A 0 A_PlaySound("Weapons/GrenadeOpen")
		TNT1 AAAA 0 A_FireCustomMissile("SmokeSpawner",0,0,0,5)
		0SCR BCDE 2
		TNT1 A 0 A_PlaySound("Weapons/GrenadeLoad")
		TNT1 AAAA 0 A_FireCustomMissile("SmokeSpawner",0,0,0,5)
		0SCR FGHHHIJK 2
		TNT1 A 0 A_PlaySound("Weapons/GrenadeClose")
		0SCR LMN 2 A_FireCustomMissile("SmokeSpawner",0,0,0,5)
	ReloadCont:
		TNT1 AAAAAA 0
		TNT1 A 0 A_JumpIfInventory("EchoRounds",4,"Refill")
		TNT1 A 0 A_JumpIfInventory("N_RocketAmmo",1,3)
		Goto Refill
        TNT1 AAAAAA 0
		TNT1 A 0 A_Giveinventory("EchoRounds",1)
		TNT1 A 0 A_Takeinventory("N_RocketAmmo",1)
		Goto ReloadCont


	Spawn:
		LAUN A -1
		Stop
	}
}

