	TNT1 A 0 A_CustomMissile ("IceCloud", 35, 0, random (0, 360), 2 | 4, random (-70, 70))
	TNT1 A 0 A_CustomMissile ("IceCloud", 35, 0, random (0, 360), 2 | 4, random (-70, 70))
	TNT1 A 0 A_CustomMissile ("IceCloud", 35, 0, random (0, 360), 2 | 4, random (-70, 70))
	TNT1 A 0 A_CustomMissile ("IceCloud", 35, 0, random (0, 360), 2 | 4, random (-70, 70))
	SENT A 20
	Goto See
  Missile:
	TNT1 A 0 A_JumpIfCloser(400, "ShotgunBlast")
	SENT A 2 A_FaceTarget
	SENT B 2 A_CustomMissile ("IceSentryShot", 35, 0, random (-2, 2), 8, random (-2, 2))
	TNT1 A 0 A_CustomMissile ("IceCloud", 35, 0, random (0, 360), 2 | 4, random (-70, 70))
	SENT A 1
