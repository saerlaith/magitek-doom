	TNT1 A 0 A_CustomMissile ("IceSpark", 35, 0, random (0, 360), 2 | 4, random (10, 50))
	TNT1 A 0 A_CustomMissile ("IceSpark", 35, 0, random (0, 360), 2 | 4, random (10, 50))
    SENT A 5 A_Look
    Goto See
