  	F1RE ABCDEFGH 1 bright
   
	F2RE ABCDEFGH 1 bright 
	TNT1 A 0 A_Explode(40, 120, 0)
	F1RE ABCDEFGH 1 bright
   
	F2RE ABCDEFGH 1 bright 
