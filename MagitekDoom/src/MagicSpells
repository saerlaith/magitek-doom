	TNT1 A 0 A_CustomMissile ("IceCloud", 35, 0, random (0, 360), 2 | 4, random (-70, 70))
	TNT1 A 0 A_CustomMissile ("IceCloud", 35, 0, random (0, 360), 2 | 4, random (-70, 70))
