		TNT1 AAAA 0 A_CustomMissile ("XDeath1", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("XDeath2", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 AA 0 A_CustomMissile ("XDeath3", 5, 0, random (0, 360), 2, random (0, 160))
	Stop
	Death.Kick:
	Death.Melee:
	Death.SuperPunch:
	    MSLG B 1 A_FaceTarget
		TNT1 A 0 A_CustomMissile ("XDeathMisslerLeg", 5, 0, random (170, 190), 2, random (20, 40))
		Stop
		
	Pain:
		TNT1 A 0
		TNT1 A 0 ThrustThingZ(0,20,0,1)
		MSLG ABCD 2
		Goto Spawn
	
	Death.ExplosiveImpact:
		TNT1 A 0
		TNT1 A 0 A_ChangeFlag("NOBLOCKMAP", 0)
		TNT1 A 0 ThrustThingZ(0,50,0,1)
		MSLG B 5
		MSLG ABCDABCDABCDABCDABCD 5 A_CheckFloor("Landz")
		Goto Landz
	Landz:
		XDT5 I 0 A_SpawnItem("Gib_MisslerLeg")
		Stop
	}
}

