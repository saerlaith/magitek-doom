//// --------------------------------------------------------------------------
//
// Missler
//
// --------------------------------------------------------------------------

ACTOR Missler : MagitekMonsterBase
{
    Health 125
    Radius 15
    Height 50
    Mass 100
    Speed 7
	damageFactor "Normal", 1.0
	damagefactor "Avoid", 0.0
	damagefactor "MINORHEAD", 1
	damagefactor "HEAD", 1
	damagefactor "Plasma2", 1
	damagefactor "IceHead", 1
	damagefactor "FireHead", 1
	damagefactor "Frenemy", 0.0
	damagefactor "Freeze", 0.0
	PainChance "Frenemy", 0
    PainChance 120
	PainChance "Stun", 255
	PainChance "Kick", 255
	PainChance "Charm", 170
	PainChance "Ice", 255
	Painchance "IceHead", 255
	PainChance "KickHead", 255
	damagefactor "KickHead", 1
	MaxStepHeight 24
	MaxDropOffHeight 32
	Scale .9
    Monster
    +FLOORCLIP
    -QUICKTORETALIATE
	SeeSound "Missler/Idle"
	ActiveSound "Missler/Idle" 
    PainSound "Missler/Pain"
    DeathSound "Missler/Death"
    HitObituary "%o was blown apart by a Missler."
    Obituary "%o was blown apart by a Missler."
    States
    {
    Spawn:
		TNT1 A 0 A_SpawnItem ("HeadshotTarget20", 5, 50,0)
        MSLR B 8 A_Look
		MSLR C 2 A_Look
		TNT1 A 0 A_SetAngle(angle + 20)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 5, 50,0)
		MSLR C 2 A_Look
		MSLR DA 4 A_Look
		TNT1 A 0 A_SetAngle(angle + 20)
        Loop
    See:
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 50,0)
        MSLR AA 2 A_Chase
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 50,0)
        MSLR AA 2 A_Chase
		
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 50,0)
        MSLR BB 2 A_Chase
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 50,0)
        MSLR BB 2 A_Chase
		
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 50,0)
        MSLR CC 2 A_Chase
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 50,0)
        MSLR CC 2 A_Chase
		
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 50,0)
        MSLR DD 2 A_Chase
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 50,0)
        MSLR DD 2 A_Chase
		
        Loop
    Missile:
		TNT1 A 0 A_Jump(100, "GrenadeShot")
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		TNT1 A 0 A_SpawnItem ("RedFlareMedium", 10, 30,0)
		MSLR E 4 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("RedFlareMedium", 10, 30,0)
		MSLR E 4 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		TNT1 A 0 A_SpawnItem ("RedFlareMedium", 10, 30,0)
		MSLR E 4 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("RedFlareMedium", 10, 30,0)
		MSLR E 4 A_FaceTarget
		TNT1 A 0 A_Jump(120, "MissileJump")
	MissileStraight:
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 50,0)
		TNT1 A 0 A_SpawnItem ("RedFlareMedium", 10, 33,0)
		MSLR F 4 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 50,0)
		TNT1 A 0 A_SpawnItem ("RedFlareMedium", 10, 36,0)
		MSLR F 4 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 50,0)
		TNT1 A 0 A_SpawnItem ("RedFlareMedium", 10, 39,0)
		MSLR F 4 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 50,0)
		TNT1 A 0 A_SpawnItem ("RedFlareMedium", 10, 42,0)
		MSLR F 4 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 50,0)
		TNT1 A 0 A_SpawnItem ("RedFlareMedium", 10, 45,0)
		TNT1 A 0 A_FaceTarget
        MSLR F 8 A_CustomMissile ("MisslerRocket", 30, 0, random (0, 0), 8, random (0, 0))
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		MSLR F 8
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		MSLR E 8
        Goto See
	MissileJump:
		TNT1 A 0 A_ChangeFlag("NOPAIN", 1)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 50,0)
		TNT1 A 0 A_SpawnItem ("RedFlareMedium", 10, 20,0)
		MSLR F 4 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 50,0)
		TNT1 A 0 A_SpawnItem ("RedFlareMedium", 10, 15,0)
		MSLR F 4 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 40,0)
		MSLR F 4 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("RedFlareMedium", 10, 10,0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 40,0)
		MSLR F 4 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("RedFlareMedium", 10, 5,0)
		TNT1 A 0 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 40,0)
        MSLR F 4 A_CustomMissile ("MisslerRocketDown", 30, 0, random (0, 0), 8, -60)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		TNT1 A 0 ThrustThingZ(0,40,0,1)
        TNT1 A 0 A_Recoil (5)
		MSLR E 4
	MidairMissile:
		TNT1 A 0 A_SpawnItem ("RedFlareMedium", 10, 30,0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		MSLR E 4 A_FaceTarget
		TNT1 A 0 A_ChangeFlag("NOGRAVITY", 1)
		TNT1 A 0 A_SpawnItem ("RedFlareMedium", 10, 30,0)
		MSLR E 4 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("RedFlareMedium", 10, 30,0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		MSLR F 4 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("RedFlareMedium", 10, 30,0)
		MSLR F 4 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("RedFlareMedium", 10, 30,0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		MSLR F 4 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("RedFlareMedium", 10, 30,0)
		MSLR F 4 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("RedFlareMedium", 10, 30,0)
		TNT1 A 0 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		TNT1 A 0 A_ChangeFlag("NOGRAVITY", 0)
		TNT1 A 0 A_Recoil (10)
        MSLR F 8 A_CustomMissile ("MisslerRocket", 30, 0, random (0, 0), 8, random (0, 0))
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		MSLR E 8
	MissileFalling:
		TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 5, 50,0)
		MSLR S 2 A_JumpIf(momz == 0, "MissileLand")
        Loop
	MissileLand:
		TNT1 A 0 A_ChangeFlag("NOPAIN", 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 45,0)
		MSLR S 4
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 40,0)
		MSLR T 4
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 35,0)
		MSLR U 4
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 40,0)
		MSLR T 4
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 45,0)
		MSLR S 4
		Goto See
	GrenadeShot:
		TNT1 A 0 A_JumpIfCloser(600, 3)
		TNT1 A 0 A_Jump(255, "Missile")
		TNT1 AAAA 0 
		MSLR G 8 A_PlaySound("Reload")
        TNT1 A 0 A_CustomMissile("RocketCaseSpawn",-30,0,-4,-4)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
        MSLR G 8
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
        MSLR D 8
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
        MSLR D 8
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		MSLR E 8
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		MSLR E 8
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		MSLR E 4 A_FaceTarget
		MSLR E 4 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		MSLR F 4 A_FaceTarget
		MSLR F 4 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		TNT1 A 0 A_FaceTarget
        MSLR F 8 A_CustomMissile ("MisslerGrenade", 30, 0, 0, CMF_OFFSETPITCH, 10)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		MSLR E 4 A_FaceTarget
		MSLR E 4 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		TNT1 A 0 A_FaceTarget
        MSLR F 8 A_CustomMissile ("MisslerGrenade", 30, 0, 0, CMF_OFFSETPITCH, 10)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		MSLR E 4 A_FaceTarget
		MSLR E 4 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		TNT1 A 0 A_FaceTarget
        MSLR F 8 A_CustomMissile ("MisslerGrenade", 30, 0, 0, CMF_OFFSETPITCH, 10)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		MSLR F 8
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		MSLR E 8
        Goto See
    Pain:
		TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 5, 50,0)
        MSLR G 6 A_Pain
        Goto See
	Pain.Stun:
		TNT1 A 0 A_ChangeFlag("NOPAIN", 1)
		TNT1 A 0 A_CustomMissile ("StunStarGen", 60, 0, 0, 2 | 4, 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget20", 5, 50,0)
        MSLR G 20 A_Pain
		TNT1 A 0 A_CustomMissile ("StunStarGen", 60, 0, 0, 2 | 4, 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget20", 5, 50,0)
		MSLR G 20
		TNT1 A 0 A_CustomMissile ("StunStarGen", 60, 0, 0, 2 | 4, 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget20", 5, 50,0)
		MSLR G 20 A_Pain
		TNT1 A 0 A_CustomMissile ("StunStarGen", 60, 0, 0, 2 | 4, 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget20", 5, 50,0)
		MSLR G 20
		TNT1 A 0 A_CustomMissile ("StunStarGen", 60, 0, 0, 2 | 4, 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget20", 5, 50,0)
		MSLR G 20 A_Pain
		TNT1 A 0 A_SpawnItem ("HeadshotTarget20", 5, 50,0)
		MSLR G 20
        Goto See
	Pain.Kick:
	Pain.KickHead:
		TNT1 A 0 A_JumpIfInventory("Kicking",1,"Pain.Cancel")
		TNT1 A 0 A_SpawnItem ("HeadshotTarget2", 5, 70,0)
		TNT1 A 0 ThrustThing(angle*256/360+128,4,0,0)
		TNT1 A 0 ThrustThingZ(0,20,0,1)
        MSLR G 2 A_Pain
		MSLR STUV 2 
		MSLR M 20
		MSLR VUTS 4 
		MSLR G 4
		TNT1 A 0 A_Giveinventory("Kicking",1)
        Goto See
	Raise:
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 50,0)
        MSLR K 1 A_KillChildren
        MSLR J 4
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8", 5, 50,0)
		MSLR IH 4
        Goto See
    Death:
		TNT1 A 0 A_Jump (95, 2)
		SENT A 0 A_SpawnItemEx ("ItemBagRocket", 0, 0, 15)
		TNT1 AAA 0
        MSLR G 8 
        MSLR S 8 A_Scream
        MSLR T 8 A_NoBlocking
		SENT A 0 A_Jump (150, 5)
		TNT1 A 0 A_SpawnItem ("DyingMissler", 5)
		Stop
		SENT AAAA 0 
		TNT1 A 0 A_Jump (220, "Death2")
		MSLR U 8 A_NoBlocking
		MSLR V 8 A_NoBlocking
        TNT1 A 0 A_SpawnItem ("DeadMissler", 5)
        Stop
	Death2:
        TNT1 A 0 A_SpawnItem ("DeadMissler2", 5)
        Stop
	Death.MinorHead:
	    TNT1 A 0
		TNT1 A 0 A_Jump (95, 2)
		SENT A 0 A_SpawnItemEx ("ItemBagRocket", 0, 0, 15)
		TNT1 AAA 0
	    TNT1 A 0 A_Scream
		MSLR H 10 A_CustomMissile ("MuchBlood2", 40, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("Brains1", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("Brains3", 50, 0, random (0, 360), 2, random (0, 160))
		MSLR I 10
		TNT1 A 0 A_CustomMissile ("SmallBrainPiece", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("SuperWallRedBlood", 46, 0, random (170, 190), 2, random (0, 40))
		SENT A 0 A_Jump (150, 5)
		TNT1 A 0 A_SpawnItem ("DyingMissler", 5)
		Stop
		SENT AAAA 0 
		MSLR JKL 6 A_NoBlocking
		TNT1 A 0 A_SpawnItem ("MediumBloodSpot")
		
        TNT1 A 0 A_SpawnItem ("DeadMissler", 5)
		Stop
	Death.Head: 
	Death.KickHead:
	    TNT1 A 0
		TNT1 A 0 A_Jump (95, 2)
		SENT A 0 A_SpawnItemEx ("ItemBagRocket", 0, 0, 15)
	    TNT1 A 0 A_Scream
		MSLR N 5 A_CustomMissile ("MuchBlood2", 40, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAAAA 0 A_CustomMissile ("Brains1", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("Brains2", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAAAA 0 A_CustomMissile ("Brains3", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("Brains7", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("Teeth", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("TeethNoBounce", 50, 0, random (0, 360), 2, random (0, 160))
		MSLR O 5 A_CustomMissile ("MuchBlood", 30, 0, random (0, 360), 2, random (0, 160))
		TNT1 AA 0 A_CustomMissile ("SmallBrainPieceFast", random (45, 55), random (5, -5), random (170, 190), 2, random (-10, 10))
		TNT1 AA 0 A_CustomMissile ("SmallBrainPiece", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 AA 0 A_CustomMissile ("SuperWallRedBlood", 46, 0, random (170, 190), 2, random (0, 40))
		TNT1 A 0 A_CustomMissile ("MuchBlood", 30, 0, random (0, 360), 2, random (0, 160))
		MSLR OOPP 5 A_NoBlocking 
		TNT1 L 0 A_CustomMissile ("MuchBlood", 30, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("MuchBlood", 30, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_SpawnItem ("BigBloodSpot")
		MSLR Q 5 A_NoBlocking 
		TNT1 A 0 A_SpawnItem ("DeadMissler_NoHead", 5)
		Stop
	Death.decaptate:
        TNT1 A 0 A_XScream
		TNT1 A 0 A_Jump (95, 2)
		SENT A 0 A_SpawnItemEx ("ItemBagRocket", 0, 0, 15)
        TNT1 A 0 A_CustomMissile ("XDeathMisslerHead", 50, 0, random (0, 360), 2, random (0, 160))
        TNT1 A 0 A_CustomMissile ("MuchBlood2", 40, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_SpawnItem ("BigBloodSpot")
		MSLR NOOPPP 10 A_CustomMissile ("MuchBlood", 40, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_SpawnItem ("BigBloodSpot")
		MSLR Q 8 A_NoBlocking
		TNT1 A 0 A_SpawnItem ("DeadMissler_NoHead", 5)
		Stop
	Death.Plasma: Death.Plasma2:
		TNT1 A 0 A_Jump (95, 2)
		SENT A 0 A_SpawnItemEx ("ItemBagRocket", 0, 0, 15)
		PBR1 A 6 A_Scream
		TNT1 AAAAAAAAAAAAAAAAAA 0 A_CustomMissile ("Ashes1", 32, 0, random (0, 360), 2, random (0, 180))
		TNT1 AAAAAAAAAAAAAAAAAA 0 A_CustomMissile ("Ashes2", 32, 0, random (0, 360), 2, random (0, 180))
		TNT1 AAAA 0 A_CustomMissile ("BluePlasmaFireNonStatic", 32, 0, random (0, 360), 2, random (0, 180))
		PBR1 B 6 A_NoBlocking
		PBR1 CDEFGH 6 A_CustomMissile ("Blood", 10, 0, random (0, 360), 2, random (0, 160))
		PBR1 HHHHHHHHHHHHHHHHHHHHHH 8 A_CustomMissile ("PlasmaSmoke", 10, 0, random (0, 360), 2, random (0, 160))
		PBR1 H -1
		Stop
	Death.Fire: Death.FireHead: 
        TNT1 A 0
		TNT1 A 0 A_Jump (95, 2)
		SENT A 0 A_SpawnItemEx ("ItemBagRocket", 0, 0, 15)
		TNT1 A 0 A_Scream
        TNT1 A 0 A_NoBlocking
		TNT1 A 0 A_SpawnItem("BurningMissler")
        Stop
	Death.Ice: Death.IceHead:  Death.Freeze: Death.Frost:
		TNT1 A 0 A_Jump (95, 2)
		SENT A 0 A_SpawnItemEx ("ItemBagRocket", 0, 0, 15)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_ChangeFlag("SOLID", 0)
		TNT1 A 0 A_SpawnItem("FrozenMissler")
		Stop
	Death.SSG: Death.Sniper:
		TNT1 A 0 A_Jump (95, 2)
		SENT A 0 A_SpawnItemEx ("ItemBagRocket", 0, 0, 15)
        TNT1 AA 0 A_CustomMissile ("MuchBlood2", 35, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_Scream
		TNT1 A 0 A_Recoil(10)
		TNT1 AAAAAA 0 A_CustomMissile ("Brains1", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("Brains2", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAAAA 0 A_CustomMissile ("Brains3", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("Brains7", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("Teeth", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("TeethNoBounce", 50, 0, random (0, 360), 2, random (0, 160))
		MSLR O 5 A_CustomMissile ("MuchBlood", 30, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAA 0 A_CustomMissile ("XDeath1", 40, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAAA 0 A_CustomMissile ("XDeath2", 40, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAAA 0 A_CustomMissile ("XDeath3", 40, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAAA 0 A_CustomMissile ("XDeath4", 40, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAA 0 A_CustomMissile ("Instestin2", 32, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("XDeath5", 40, 0, random (0, 360), 2, random (0, 160))
		TNT1 AA 0 A_CustomMissile ("SmallBrainPieceFast", random (45, 55), random (5, -5), random (170, 190), 2, random (-10, 10))
		TNT1 AA 0 A_CustomMissile ("SmallBrainPiece", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 AA 0 A_CustomMissile ("SuperWallRedBlood", 46, 0, random (170, 190), 2, random (0, 40))
		TNT1 A 0 A_CustomMissile ("MuchBlood", 30, 0, random (0, 360), 2, random (0, 160))
        TNT1 A 0 A_SpawnItem ("MisslerLowerHalf")
		TNT1 A 0 A_SpawnItem ("MediumBloodSpot")
		Stop
	Death.Tear: Death.cutless: Death.cut: Death.HKFT:
		TNT1 A 0 A_Jump (95, 2)
		SENT A 0 A_SpawnItemEx ("ItemBagRocket", 0, 0, 15)
        TNT1 AA 0 A_CustomMissile ("MuchBlood2", 35, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_Scream
		TNT1 A 0 A_CustomMissile ("RipMissler", 0, 0, random (0, 360), 2, random (0, 160))
        TNT1 A 0 A_SpawnItem ("MisslerLowerHalf")
		TNT1 A 0 A_SpawnItem ("MediumBloodSpot")
		Stop
	Death.LandMine: Death.ExplosiveImpact: Death.ExtremePunches: XDeath:
		TNT1 A 0
		TNT1 A 0 ThrustThingZ(0,70,0,1)
		TNT1 A 0 A_Recoil(10)
		TNT1 A 0 A_Jump (95, 2)
		SENT A 0 A_SpawnItemEx ("ItemBagRocket", 0, 0, 15)
		TNT1 AAA 0
		TNT1 AA 0 A_CustomMissile ("XDeath1", 40, 0, random (0, 360), 2, random (0, 160))
		TNT1 AA 0 A_CustomMissile ("SuperWallRedBlood", 46, 0, random (170, 190), 2, random (0, 40))
		SENT A 0 A_Jump (150, 5)
		TNT1 A 0 A_SpawnItem ("DyingMissler", 5)
		Stop
		SENT AAAA 0 
		TNT1 AAAAAAAAAAA 0 A_CustomMissile ("BrainBlood", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("XDeathMisslerLeg", 32, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("XDeathMisslerArm", 62, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAA 0 A_CustomMissile ("XDeath1", 40, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_Scream
	    MSLR Z 4
		MSLR XZXZXZXZXZXZ 4 A_CheckFloor ("Dead.Landmine")
		TNT1 A 0 A_SpawnItem ("DeadMisslerTorso")
		Stop
	Dead.Landmine:
		TNT1 AAAA 0 A_SpawnItem ("Blood")
		TNT1 A 0 A_SpawnItem ("Ploft")
		TNT1 A 0 A_SpawnItem ("DeadMisslerTorso")
		Stop
    }
}

Actor DyingMissler : DyingSergeant
{
    Radius 20
    Height 18
	Scale .9
    +SHOOTABLE
    +NOTELEPORT
    +MOVEWITHSECTOR
	+SOLID
	+NOPAIN
	PainSound "Missler/Pain"
    DeathSound "Missler/Death"
	damagefactor "Crush", 50.0
    Mass 300
