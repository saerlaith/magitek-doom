Actor HeadshotAOE : MNEFFECT
{ 
  RenderStyle "Normal"
  Alpha 1
  Scale .7
  Gravity 0
  Translation "0:255=%[0,0,0]:[2.0,0.0,0.0]"
  States
 {
  Spawn:
	SHOK ABCDEFGHIJKLMNOPQRS 1 BRIGHT LIGHT ("HOMEFIREBALL")
    Goto Death
  Death:
    Stop
  }
}

Actor HeadshotConfirm : MNEFFECT
{
  Speed 3
  Gravity 0
  BounceType "Doom" 
  Alpha 0.7
  Scale 1
  Gravity 0.1
  +NOTIMEFREEZE
  States
 {
  Spawn:
	TNT1 A 0
	TNT1 A 0 A_PlaySound("General/HeadShotHit")	
	HSEP A 5 BRIGHT
	HSEP AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA 1 BRIGHT A_FadeOut(0.01)
    Goto Death
  Death:
    Stop
  }
}

