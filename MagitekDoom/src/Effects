//Special Effects-----------------------------------------------------

Actor MNEFFECT 
{
  Radius 1
  Height 1
  +DROPOFF
  -NOGRAVITY
  +CANNOTPUSH
  -FLOORCLIP
  +NOTELEPORT
  +NOBLOCKMAP
  +MOVEWITHSECTOR
  RenderStyle "Add"
}

