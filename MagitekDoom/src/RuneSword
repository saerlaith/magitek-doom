		BFSF A 2 Offset(-1,40)
		TNT1 A 0 A_Refire
		Goto LightSlash
		
	  
	BasicSlash:
		TNT1 A 0 A_jumpifinventory("SwordType",2,"ThirdSlash")
		TNT1 A 0 A_jumpifinventory("SwordType",1,"SecondSlash")
		NULL A 0 A_JumpIfHealthLower(35, "BasicSlashHeal")
		BFSF A 2 Offset(-20,40)
		TNT1 A 0 A_PlaySound("Melee/SwingHeavy", 1, 1.0)
		BFSF B 2 Offset(-35,50)
