		BFSF C 0 A_FireCustomMissile("WaveSpin", 0, 0, 0, 5)
		BFSF C 1 Offset(-90,55) A_SetAngle(20 + angle)
		BFSF C 0 A_FireCustomMissile("TimeWideSlash", 0, 0, 0, 5)
		BFSF C 0 A_FireCustomMissile("WaveSpin", 0, 0, 0, 5)
		BFSF C 1 Offset(-90,60) A_SetAngle(20 + angle)
		BFSF C 0 A_FireCustomMissile("TimeWideSlash", 0, 0, 0, 5)
		BFSF C 0 A_FireCustomMissile("WaveSpin", 0, 0, 0, 5)
		BFSF C 1 Offset(-100,60) A_SetAngle(20 + angle)
		BFSF C 0 A_FireCustomMissile("TimeWideSlash", 0, 0, 0, 5)
		BFSF C 0 A_FireCustomMissile("WaveSpin", 0, 0, 0, 5)
		BFSF C 1 Offset(-100,65)A_SetAngle(20 + angle)
		BFSF C 0 A_FireCustomMissile("TimeWideSlash", 0, 0, 0, 5)
		BFSF C 0 A_FireCustomMissile("WaveSpin", 0, 0, 0, 5)
		BFSF C 1 Offset(-110,65) A_SetAngle(20 + angle)
		BFSF C 0 A_FireCustomMissile("TimeWideSlash", 0, 0, 0, 5)
		BFSF C 0 A_FireCustomMissile("WaveSpin", 0, 0, 0, 5)
		BFSF C 1 Offset(-120,70) A_SetAngle(20 + angle)
		BFSF C 0 A_FireCustomMissile("TimeWideSlash", 0, 0, 0, 5)
		BFSF C 0 A_FireCustomMissile("WaveSpin", 0, 0, 0, 5)
		BFSF C 1 Offset(-130,75) A_SetAngle(20 + angle)
		BFSF C 0 A_FireCustomMissile("TimeWideSlash", 0, 0, 0, 5)
		BFSF C 0 A_FireCustomMissile("WaveSpin", 0, 0, 0, 5)
		BFSF C 1 Offset(-140,80) A_SetAngle(20 + angle)
		BFSF C 0 A_FireCustomMissile("TimeWideSlash", 0, 0, 0, 5)
		BFSF C 0 A_FireCustomMissile("WaveSpin", 0, 0, 0, 5)
		BFSF D 5 Offset(20,40) A_FireCustomMissile("CutBang", 0, 0, 0, 5)
		TNT1 A 0 a_unsetinvulnerable
		TNT1 A 0 A_Takeinventory("Kicking",1)
		TNT1 A 10 Offset(-20,40)
		goto Ready+10
		
	Reload:
		TNT1 A 0 A_TakeInventory("ChargeShot", 50)
		TNT1 A 0 A_Takeinventory("SpecialShot",9)
		TNT1 A 0 A_jumpifinventory("PowerStrength",1,"PowerReload")
		TNT1 A 0 A_Takeinventory("Reloading",5)
		TNT1 A 0 a_setinvulnerable
		BFSS B 2 Offset(-60,80) A_SpawnItemEx("SwordParry", 15, 0, 15, 0)
		BFSS B 2 Offset(-10,40) A_SpawnItemEx("SwordParry", 15, 0, 15, 0)
		TNT1 A 0 a_unsetinvulnerable
		BFSS BBB 2 Offset(30,10) A_SpawnItemEx("SwordParry", 15, 0, 15, 0)
		TNT1 A 0 A_FireCustomMissile ("WaveBreakSparkle", random(-30,30), 0, 0, 5)
		TNT1 A 0 A_jumpifinventory("Reloading",1,"ReloadFinish")
		TNT1 A 0 A_Giveinventory("MeleeDefensePower",1)
		BFSS BBBBB 2 A_SpawnItemEx("SwordParry", 15, 0, 15, 0) 
		TNT1 A 0 A_FireCustomMissile ("WaveBreakSparkle", random(-30,30), 0, 0, 5)
		TNT1 A 0 A_jumpifinventory("Reloading",1,"ReloadFinish")
		TNT1 A 0 A_Giveinventory("MeleeDefensePower",1)
		BFSS BBBBB 2 A_SpawnItemEx("SwordParry", 15, 0, 15, 0) 
		TNT1 A 0 A_FireCustomMissile ("WaveBreakSparkle", random(-30,30), 0, 0, 5)
		TNT1 A 0 A_jumpifinventory("Reloading",1,"ReloadFinish")
		TNT1 A 0 A_Takeinventory("SpecialShot",9)
		goto ReloadFinish
	PowerReload:
		TNT1 A 0 A_Takeinventory("Reloading",5)
		TNT1 A 0 a_setinvulnerable
		BFSS B 2 Offset(-60,80) A_SpawnItemEx("SwordParry", 15, 0, 15, 0)
		BFSS B 2 Offset(-10,40) A_SpawnItemEx("SwordParry", 15, 0, 15, 0)
		TNT1 A 0 a_unsetinvulnerable
		BFSS BBB 2 Offset(30,10) A_SpawnItemEx("SwordParry", 15, 0, 15, 0)
		BFSS BBBBB 2 A_SpawnItemEx("SwordParry", 15, 0, 15, 0)
		TNT1 A 0 A_FireCustomMissile ("WaveBreakSparkle", random(-30,30), 0, 0, 5)
		TNT1 A 0 A_jumpifinventory("Reloading",1,"ReloadFinish")
		TNT1 A 0 A_Giveinventory("MeleeDefensePower",1)
		BFSS BBBBB 2 A_SpawnItemEx("SwordParry", 15, 0, 15, 0) 
		TNT1 A 0 A_FireCustomMissile ("WaveBreakSparkle", random(-30,30), 0, 0, 5)
		TNT1 A 0 A_jumpifinventory("Reloading",1,"ReloadFinish")
		TNT1 A 0 A_Giveinventory("MeleeDefensePower",1)
		BFSS BBBBB 2 A_SpawnItemEx("SwordParry", 15, 0, 15, 0) 
		TNT1 A 0 A_FireCustomMissile ("WaveBreakSparkle", random(-30,30), 0, 0, 5)
		TNT1 A 0 A_jumpifinventory("Reloading",1,"ReloadFinish")
		TNT1 A 0 A_Giveinventory("MeleeDefensePower",1)
		BFSS BBBBB 2 A_SpawnItemEx("SwordParry", 15, 0, 15, 0)
		TNT1 A 0 A_FireCustomMissile ("WaveBreakSparkle", random(-30,30), 0, 0, 5)
		TNT1 A 0 A_jumpifinventory("Reloading",1,"ReloadFinish")
		TNT1 A 0 A_Giveinventory("MeleeDefensePower",1)
		BFSS BBBBB 2 A_SpawnItemEx("SwordParry", 15, 0, 15, 0) 
		TNT1 A 0 A_FireCustomMissile ("WaveBreakSparkle", random(-30,30), 0, 0, 5)
		TNT1 A 0 A_jumpifinventory("Reloading",1,"ReloadFinish")
		TNT1 A 0 A_Giveinventory("MeleeDefensePower",1)
		BFSS BBBBB 2 A_SpawnItemEx("SwordParry", 15, 0, 15, 0) 
		TNT1 A 0 A_FireCustomMissile ("WaveBreakSparkle", random(-30,30), 0, 0, 5)
		TNT1 A 0 A_jumpifinventory("Reloading",1,"ReloadFinish")
		TNT1 A 0 A_Giveinventory("MeleeDefensePower",1)
		BFSS BBBBB 2 A_SpawnItemEx("SwordParry", 15, 0, 15, 0) 
		TNT1 A 0 A_FireCustomMissile ("WaveBreakSparkle", random(-30,30), 0, 0, 5)
		TNT1 A 0 A_jumpifinventory("Reloading",1,"ReloadFinish")
		TNT1 A 0 A_Takeinventory("SpecialShot",9)
	ReloadFinish:
		TNT1 A 0 A_jumpifinventory("SpecialShot",1,"ReflectShot")
		TNT1 A 0 A_Takeinventory("Reloading",5)
		BFSS B 2 Offset(30,10)
		BFSS B 2 Offset(10,20)
		BFSS B 2 Offset(-10,40)
		BFSS B 2 Offset(-30,60)
		BFSS B 2 Offset(-60,80)
		TNT1 A 0 Offset(-20,40)
		goto Ready+10
		
	ReflectShot:
		TNT1 A 0 A_Takeinventory("SpecialShot",9)
		TNT1 A 0 A_AlertMonsters (500)
		TNT1 A 0 A_jumpifinventory("PowerStrength",1,"PowerReflectShot")
		TNT1 A 0 A_PlaySound("Melee/WaveSpawn", 1, 1.0)
		TNT1 AAAAA 0 A_FireCustomMissile ("WaveBreakSparkle", random(-30,30), 0, 0, 5)
		BFSF F 4 Offset(200,70) a_setinvulnerable
		TNT1 A 0 BRIGHT A_FireCustomMissile("AlerterLowChance")
		BFSF G 2 Offset(130,70)
		BFSF G 2 Offset(200,50)
		TNT1 A 0 A_FireCustomMissile("WaveCutMP", 0, 0, 0, 5)
		BFSF H 2 Offset(100,40)
		BFSF I 4 Offset(20,25) 
		TNT1 A 8 Offset(-20,40) a_unsetinvulnerable
		TNT1 A 0 A_Takeinventory("Reloading",5)
		goto Ready+10
		
	PowerReflectShot:
		TNT1 A 0 A_PlaySound("Melee/WaveSpawn", 1, 1.0)
		TNT1 AAAAA 0 A_FireCustomMissile ("WaveBreakSparkle", random(-30,30), 0, 0, 5)
		BFSF F 4 Offset(200,70) a_setinvulnerable
		TNT1 A 0 BRIGHT A_FireCustomMissile("AlerterLowChance")
		BFSF G 2 Offset(130,70)
		BFSF G 2 Offset(200,50)
		TNT1 A 0 A_FireCustomMissile("WaveCut", -8, 0, 0, 5)
		TNT1 A 0 A_FireCustomMissile("WaveCutMP", 0, 0, 0, 5)
		TNT1 A 0 A_FireCustomMissile("WaveCut", 8, 0, 0, 5)
		BFSF H 2 Offset(100,40)
		BFSF I 4 Offset(20,25) 
		TNT1 A 8 Offset(-20,40) a_unsetinvulnerable
		TNT1 A 0 A_Takeinventory("Reloading",5)
		goto Ready+10
		
    Taunt:
        TNT1 A 5
		FUCK A 2
        FUCK B 2 A_PlaySound("FUCK", 1)
        FUCK C 2 A_AlertMonsters
		FUCK D 15 A_Takeinventory("Taunting",1)
        FUCK CBA 5
        TNT1 A 5
		goto ready+35
	}
}

