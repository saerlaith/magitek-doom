ACTOR FireChargeSlash
{
  height 8
  radius 8
  damage (20)
  damageType Fire
  Scale 1
  PROJECTILE
  Speed 60
  -NOGRAVITY
  Gravity 0.9
  +BLOODSPLATTER
  +NOEXTREMEDEATH
  +SEEKERMISSILE
  +PAINLESS
   States
   {
	Spawn:
      NEDL A 0 
      TNT1 A 2 A_CustomMissile("FlameTrail2", 0, 0, random (0, 360), 2 | 4, random (-50, 50))
      Stop
    Death:
	  D2:
      NEDL A 0 A_NoGravity
	  EXPL A 0 A_CustomMissile ("BurningSpot", 0, 0, 0, 2, 0)
      NEDL A 0 A_JumpIf(waterlevel == 3, 3)
      NEDL A 0 A_PlaySound("Melee/Clash", 1, 1.0)
      NEDL A 0 A_CustomMissile("NeedleSmoke",0,0,0)
      Goto Death+8
      NEDL A 0 A_PlaySound("weapons/needleunderwall")
      NEDL AAA 0 A_SpawnItemEx("NeedleBubble",-4,0,0,1,0,1,random(170,190), 128, 0)
      Stop
    XDeath:
	  SLSH A 0 A_PlaySound("MBow/ArrowHit", 1, 1.0)
	  TNT1 A 0 A_SetScale(0.6)
	  TNT1 A 0 A_SeekerMissile(90,90,1,255,0)
	  SLSH A 0 A_PlaySound("custom/fireBreak", 1, 1.0)
