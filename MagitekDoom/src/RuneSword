		TNT1 A 0 A_FireCustomMissile("BaseSlash", 0, 0, 0, 0, 0, 0)
		TNT1 A 0 A_Jump (256, 3)
		TNT1 A 0
		TNT1 A 0 A_FireCustomMissile("TimeBaseSlash", 0, 0, 0, 0, 0, 0)
		BFSF C 2 Offset(-40,40)
		TNT1 A 0 A_FireCustomMissile("TimeBaseSlash", 20, 0, 0, 0, 0, -10)
		BFSF C 2 Offset(-90,60)
		TNT1 A 0 A_Giveinventory("SwordType",1)
		BFSF C 2 Offset(-140,80)
		BFSF DDDD 1 Offset(20,40) A_Refire
		BFSF DDDD 1 Offset(20,40) A_Refire
		TNT1 AA 1 Offset(-20,40) A_Refire
		TNT1 A 0 A_Takeinventory("SwordType",1)
		goto Ready+10
		
	SecondSlash:
		NULL A 0 A_JumpIfHealthLower(35, "SecondSlashHeal")
		BFSF C 2 Offset(-140,80)
		BFSF F 4 Offset(200,70)
