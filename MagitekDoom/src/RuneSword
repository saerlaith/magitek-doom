ACTOR EarthSpikeSword
{
	Scale 1
	Radius 6
	Height 8
	Speed 0
	Damage 0
	DamageType "Earth"
	Projectile
	Renderstyle Translucent
	Alpha 1
	+FLOORHUGGER
	+NOCLIP
	+NOEXTREMEDEATH
	States
	{
	Spawn:
		SPIK A 0
		TNT1 A 0 A_CustomMissile ("RockBreakPart", 10, 0, random (0, 360), 2 | 4, random (30, 90))
		TNT1 A 0 A_CustomMissile ("RockBreakPart2", 10, 0, random (0, 360), 2 | 4, random (30, 90))
		TNT1 A 0 A_CustomMissile ("RockBreakPart3", 10, 0, random (0, 360), 2 | 4, random (30, 90))
		TNT1 A 0 A_CustomMissile ("RockBreakPart", 10, 0, random (0, 360), 2 | 4, random (30, 90))
		TNT1 A 0 A_CustomMissile ("RockBreakPart2", 10, 0, random (0, 360), 2 | 4, random (30, 90))
		TNT1 A 0 A_CustomMissile ("RockBreakPart3", 10, 0, random (0, 360), 2 | 4, random (30, 90))
		TNT1 A 0 A_CustomMissile ("RockBreakPart", 10, 0, random (0, 360), 2 | 4, random (30, 90))
		TNT1 A 0 A_CustomMissile ("RockBreakPart2", 10, 0, random (0, 360), 2 | 4, random (30, 90))
		TNT1 A 0 A_CustomMissile ("RockBreakPart3", 10, 0, random (0, 360), 2 | 4, random (30, 90))
		SPIK A 1 A_PlaySound("Magic/Earth2", 1, 1.0)
		SPIK BCD 1
		SPIK E 8 A_Explode(100,150,false)
		SPIK DCB 3 
		SPIK A 100
		SPIK AAAAAAAAAAAAAAAAAAAA 1 A_FadeOut(0.05)
		Stop
	Death:
		TNT1 A 1
		Stop
	}
}

ACTOR LightChargeSlash : BaseSlash
{
  height 3
  radius 15
  damage (100)
  speed 40
  Scale 1.5
  DamageType Cut
  PROJECTILE
   States
   {
	Spawn: 
		TNT1 A 0 bright
		Goto ContinueFrames
	ContinueFrames:
		SWAV A 1 Bright A_CustomMissile ("LightWaveTrail", 0, 0, 0)
		Loop
    Death: Crash:
		TNT1 A 0 A_CustomMissile ("LightWaveBreak", 0, 0, random (0, 360), 2 | 4, random (-50, 50))
		TNT1 A 0 A_CustomMissile ("LightWaveBreak", 0, 0, random (0, 360), 2 | 4, random (-50, 50))
		TNT1 A 0 A_CustomMissile ("LightWaveBreak", 0, 0, random (0, 360), 2 | 4, random (-50, 50))
		SLSH A 1 A_PlaySound("Melee/Clash", 1, 1)
		SLSH BCDEFG 1
		Stop
	XDeath: 
		TNT1 A 0
		SLSH A 3 A_PlaySound("Melee/SlashHeavy", 1, 1)
		SLSH BCDEFG 3
		Stop
   }
}

