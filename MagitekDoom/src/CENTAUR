	    TNT1 A 0 A_JumpIf(waterlevel > 1, "Death")
        CNTT A 8 A_CustomMissile("BloodTrails",0,0,180,2)
        Goto SpawnOther
    SpawnOther:
        TNT1 A 0 A_SpawnItem("NashGore_BloodSpot",0,0,0,1)
	    TNT1 A 0 A_JumpIf(waterlevel > 1, "Death")
        CNTT B 12 A_CustomMissile("BloodTrails",0,0,180,2)
        Loop
    Death:
        TNT1 A 0 A_SpawnItem("NashGore_BloodSpot",0,0,0,1)
        CNTT B 0 A_SpawnItem("GibHalfCentaur",0,0,0,1)
        Stop
    }
