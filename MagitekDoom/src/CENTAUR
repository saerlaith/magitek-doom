// --------------------------------------------------------------------------

ACTOR Centaur : MagitekMonsterBase
{
    Health 1000
    Radius 25
    Height 70
    Mass 450
    Speed 12
	MinMissileChance 150
	damagefactor "Avoid", 0.0
	damagefactor "MINORHEAD", 1
	damagefactor "HEAD", 1
	damagefactor "Frenemy", 0.0
	damagefactor "Freeze", 0.0
	PainChance "Frenemy", 0
    PainChance 50
	PainChance "Stun", 255
	PainChance "Kick", 255
	PainChance "Ice", 255
	Painchance "IceHead", 255
	PainChance "Charm", 50
	PainChance "KickHead", 255
	PainChance "Shock", 150
	PainChance "ShockForce", 200
	damagefactor "KickHead", 1
	MeleeRange 150
	MaxStepHeight 24
	MaxDropOffHeight 32
	Scale 1.2
    Monster
    +FLOORCLIP
    +QUICKTORETALIATE
	+DONTHURTSPECIES
    SeeSound "Centaur/See"
    PainSound "Centaur/Pain"
    DeathSound "Centaur/Death"
    ActiveSound "Centaur/See"
	Translation 2
    States
    {
    Spawn: SpawnNormal:
		TNT1 A 0 A_SpawnItem ("HeadshotTarget20b", 5, 70,0)
        CNTR B 8 A_Look
		CNTR C 2 A_Look
		TNT1 A 0 A_SetAngle(angle - 20)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10b", 5, 70,0)
		CNTR C 2 A_Look
		CNTR DA 4 A_Look
		TNT1 A 0 A_SetAngle(angle - 20)
        Loop
    See:
		TNT1 A 0 A_JumpIfInventory("AngryToggle",1,2)
		TNT1 A 0 A_JumpIfInventory("AngryCharge",1,"Rage")
		TNT1 AA 0
		TNT1 A 0 A_JumpIfInventory("AngryCharge",1,"RageSee")
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4b", 5, 70,0)
        CNTR AA 2 A_Chase
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4b", 5, 70,0)
        CNTR AA 2 A_Chase
		
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4b", 5, 70,0)
        CNTR BB 2 A_Chase
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4b", 5, 70,0)
        CNTR BB 2 A_Chase
		
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4b", 5, 70,0)
        CNTR CC 2 A_Chase
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4b", 5, 70,0)
        CNTR CC 2 A_Chase
		
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4b", 5, 70,0)
        CNTR DD 2 A_Chase
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4b", 5, 70,0)
        CNTR DD 2 A_Chase
		
        Loop
	WalkAfter:
		TNT1 A 0 A_JumpIfInventory("AngryToggle",1,2)
		TNT1 A 0 A_JumpIfInventory("AngryCharge",1,"Rage")
		TNT1 AA 0
		TNT1 A 0 A_JumpIfInventory("AngryCharge",1,"RageAfter")
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4b", 5, 70,0)
        CNTR AA 2 A_Chase ("", "")
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4b", 5, 70,0)
        CNTR AA 2 A_Chase ("", "")
		
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4b", 5, 70,0)
        CNTR BB 2 A_Chase ("", "")
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4b", 5, 70,0)
        CNTR BB 2 A_Chase ("", "")
		
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4b", 5, 70,0)
        CNTR CC 2 A_Chase ("", "")
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4b", 5, 70,0)
        CNTR CC 2 A_Chase ("", "")
		
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4b", 5, 70,0)
        CNTR DD 2 A_Chase ("", "")
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4b", 5, 70,0)
        CNTR DD 2 A_Chase ("", "")
		
        Goto See
	RageSee:
		TNT1 A 0 A_SpawnItem ("HeadshotTarget2b", 5, 70,0)
		TNT1 A 0 Thing_SetTranslation(0, 1)
        CNTR AA 1 A_Chase
		TNT1 A 0 A_CustomMissile ("DangerMark", random (30, 70), 0, random (0, 360), 2 | 4, random (-50, 50))
		TNT1 A 0 A_SpawnItem ("HeadshotTarget2b", 5, 70,0)
        CNTR AA 1 A_Chase 
		TNT1 A 0 A_CustomMissile ("GreenGeyserFlame", random (30, 70), 0, random (0, 360), 2 | 4, random (-50, 50))
		
		TNT1 A 0 A_SpawnItem ("HeadshotTarget2b", 5, 70,0)
        CNTR BB 1 A_Chase
		TNT1 A 0 A_CustomMissile ("DangerMark", random (30, 70), 0, random (0, 360), 2 | 4, random (-50, 50))
		TNT1 A 0 A_SpawnItem ("HeadshotTarget2b", 5, 70,0)
        CNTR BB 1 A_Chase
		TNT1 A 0 A_CustomMissile ("GreenGeyserFlame2", random (30, 70), 0, random (0, 360), 2 | 4, random (-50, 50))
		
		TNT1 A 0 A_SpawnItem ("HeadshotTarget2b", 5, 70,0)
        CNTR CC 1 A_Chase
		TNT1 A 0 A_CustomMissile ("DangerMark", random (30, 70), 0, random (0, 360), 2 | 4, random (-50, 50))
		TNT1 A 0 A_SpawnItem ("HeadshotTarget2b", 5, 70,0)
        CNTR CC 1 A_Chase
		TNT1 A 0 A_CustomMissile ("GreenGeyserFlame3", random (30, 70), 0, random (0, 360), 2 | 4, random (-50, 50))
		
		TNT1 A 0 A_SpawnItem ("HeadshotTarget2b", 5, 70,0)
        CNTR DD 1 A_Chase
		TNT1 A 0 A_SpawnItem ("HeadshotTarget2b", 5, 70,0)
        CNTR DD 1 A_Chase
		
        Loop
	RageAfter:
		TNT1 A 0 A_SpawnItem ("HeadshotTarget2b", 5, 70,0)
        CNTR AA 1 A_Chase ("", "")
		TNT1 A 0 A_CustomMissile ("DangerMark", random (30, 70), 0, random (0, 360), 2 | 4, random (-50, 50))
		TNT1 A 0 A_SpawnItem ("HeadshotTarget2b", 5, 70,0)
        CNTR AA 1 A_Chase ("", "") 
		TNT1 A 0 A_CustomMissile ("GreenGeyserFlame", random (30, 70), 0, random (0, 360), 2 | 4, random (-50, 50))
		
		TNT1 A 0 A_SpawnItem ("HeadshotTarget2b", 5, 70,0)
        CNTR BB 1 A_Chase ("", "")
		TNT1 A 0 A_CustomMissile ("DangerMark", random (30, 70), 0, random (0, 360), 2 | 4, random (-50, 50))
		TNT1 A 0 A_SpawnItem ("HeadshotTarget2b", 5, 70,0)
        CNTR BB 1 A_Chase ("", "")
		TNT1 A 0 A_CustomMissile ("GreenGeyserFlame2", random (30, 70), 0, random (0, 360), 2 | 4, random (-50, 50))
		
		TNT1 A 0 A_SpawnItem ("HeadshotTarget2b", 5, 70,0)
        CNTR CC 1 A_Chase ("", "")
		TNT1 A 0 A_CustomMissile ("DangerMark", random (30, 70), 0, random (0, 360), 2 | 4, random (-50, 50))
		TNT1 A 0 A_SpawnItem ("HeadshotTarget2b", 5, 70,0)
        CNTR CC 1 A_Chase ("", "")
		TNT1 A 0 A_CustomMissile ("GreenGeyserFlame3", random (30, 70), 0, random (0, 360), 2 | 4, random (-50, 50))
		
		TNT1 A 0 A_SpawnItem ("HeadshotTarget2b", 5, 70,0)
        CNTR DD 1 A_Chase ("", "")
		TNT1 A 0 A_SpawnItem ("HeadshotTarget2b", 5, 70,0)
        CNTR DD 1 A_Chase ("", "")
		
        Goto RageSee
	Rage:
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8b", 5, 70,0)
		TNT1 A 0 A_GiveInventory("AngryToggle",1)
		TNT1 A 0 A_ChangeFlag("NOPAIN", 1)
		TNT1 A 0 Thing_SetTranslation(0, 1)
		TNT1 A 0 A_PlaySound("Centaur/Rage", CHAN_NOPAUSE, .7, 0, ATTN_NONE)
		TNT1 A 0 A_Quake(4, 100, 0, 1000)
		TNT1 A 0 A_SpawnItemEx("CentaurRage",80,0,0,0,0,0,0,32,0)
		TNT1 A 0 A_SpawnItemEx("CentaurRage",-40,0,0,0,0,0,0,32,0)
		TNT1 A 0 A_SpawnItemEx("CentaurRage",0,40,0,0,0,0,0,32,0)
		TNT1 A 0 A_SpawnItemEx("CentaurRage",0,-40,0,0,0,0,0,32,0)
		CNTR E 8
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8b", 5, 70,0)
        CNTR E 8
		TNT1 A 0 A_SpawnItemEx("CentaurRage",80,0,0,0,0,0,0,32,0)
		TNT1 A 0 A_SpawnItemEx("CentaurRage",-40,0,0,0,0,0,0,32,0)
