		TNT1 A 0 A_SpawnItemEx("CentaurRage",0,-40,0,0,0,0,0,32,0)
		CNTR E 8
		TNT1 A 0 A_SpawnItem ("HeadshotTarget8b", 5, 70,0)
        CNTR F 8
		TNT1 A 0 A_SpawnItemEx("CentaurRage",80,0,0,0,0,0,0,32,0)
		TNT1 A 0 A_SpawnItemEx("CentaurRage",-40,0,0,0,0,0,0,32,0)
