	TNT1 A 0 A_Jump (100, "FlyUp", "FlyDown")
    loop
  FlyUp:
	TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 45,0)
	TNT1 A 0 ThrustThingZ(0,10,0,1)
    GRGY AA 2 
	TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 45,0)
	GRGY BB 2 
	TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 45,0)
	GRGY CC 2 
	TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 45,0)
	GRGY DD 2 
