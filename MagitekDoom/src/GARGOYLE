actor Gargoyle : MagitekMonsterBase
{
  obituary "%o got torn apart by an Echo Gargoyle."
  Scale .8
  Health 250
  Radius 20
  Height 45
  Speed 8
  FloatSpeed 10
  Mass 100
  PainChance 30
  damagefactor "Frenemy", 0.0
  damagefactor "Freeze", 0.0
