		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 5, 40,0)
		GRGY H 10
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 5, 40,0)
		GRGY H 10
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 5, 40,0)
		GRGY EEFFG 2
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 5, 40,0)
		GRGY GFEAA 2
		TNT1 A 0 A_ChangeFlag("NOPAIN", 0)
		TNT1 A 0 A_ChangeFlag("FLOAT", 1)
