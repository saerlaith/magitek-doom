	{
	Spawn:
	TNT1 A 0
	TNT1 A 0 A_GiveInventory("TargetIsAMarine")
	PLAY A 1 A_Look
	Goto See
	See:
	    TNT1 A 0 A_SpawnItemEx("FootStep", 0, 0, 10, 0, 0)
		PLAY AAAABBBB 1 A_Chase
		TNT1 A 0 A_JumpIfMasterCloser("XDeath")
		PLAY CCCCDDDD 1 A_Chase
		
		Goto See
	
	Pain.Kick:
		PLAY A 2 A_FaceTarget
