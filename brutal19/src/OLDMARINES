
ACTOR Marine: ZombieMan Replaces MarinePistol
{
	Game Doom
	Radius 12
	Height 52
    Speed 5
    Health 150
	PainChance 200
	PainChance "Kick", 255
	Monster
    +FRIENDLY
	+FLOORCLIP
	+NORADIUSDMG
	+QUICKTORETALIATE
	+MISSILEMORE
	+MISSILEEVENMORE
    +NOBLOCKMONST 
    +DONTHURTSPECIES
    +FIXMAPTHINGPOS 
	+LOOKALLAROUND
    Scale 0.98
	AttackSound "none"
	MaxStepHeight 56
	MaxDropOffHeight 56
    BloodType "NashGore_Blood"
		    damagefactor "FriendBullet", 0.0
