		HEAD P 1 A_SpawnItem("100SmallFireFocusSpawnerz")
		HEAD PPPPPPPPPPPPPPPPPPPPPPPP 6 A_CustomMissile("PlasmaSmoke", 15, 0, random (0, 360), 2, random (50, 130))
		HEAD P -1
        Stop
    }
